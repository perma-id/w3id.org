    <owl:Ontology rdf:about="https://w3id.org/gcdfo/salmon">
        <owl:versionIRI rdf:resource="https://w3id.org/gcdfo/salmon/1.0.0"/>
        <obo:IAO_0000115 xml:lang="en">Hybrid OWL+SKOS ontology for DFO salmon data. Uses SKOS for controlled vocabularies (methods, criteria) and OWL for formal relationships and events.</obo:IAO_0000115>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-09-06</terms:created>
        <terms:creator xml:lang="en">Fishery &amp; Assessment Data Section (FADS) Data Stewardship Unit</terms:creator>
        <terms:description xml:lang="en">OWL + SKOS ontology for DFO salmon data: stocks/CUs/MUs, survey events, escapement measurements, and GSI constructs; aligns to DwC via rdfs:subClassOf; hybrid approach with SKOS for methods and OWL for events.</terms:description>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-12-08</terms:modified>
        <terms:publisher xml:lang="en">Fisheries and Oceans Canada (DFO) Pacific Region Science Branch</terms:publisher>
        <terms:title xml:lang="en">GC DFO Salmon Ontology</terms:title>
        <rdfs:seeAlso rdf:resource="https://github.com/dfo-pacific-science/salmon-ontology"/>
        <owl:versionInfo>1.0.0 (hybrid OWL+SKOS approach)</owl:versionInfo>
    </owl:Ontology>
    


