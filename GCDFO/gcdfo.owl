        <obo:IAO_0000119 rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/oa#"/>
        <rdfs:label xml:lang="en">Annotation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#AlgorithmThreshold -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#AlgorithmThreshold">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000030"/>
        <obo:IAO_0000115 xml:lang="en">Threshold value used by the rapid-status algorithm (such as Learning Tree 3) that may differ from expert-defined benchmarks to incorporate precautionary buffers.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">DFO (2024). Rapid Status Approximations for Pacific Salmon Derived from Integrated Expert Assessments under Canada’s Wild Salmon Policy. CSAS Science Response 2024/004.</obo:IAO_0000119>
        <terms:source rdf:resource="https://www.dfo-mpo.gc.ca/csas-sccs/Publications/ScR-RS/2024/2024_004-eng.pdf"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Algorithm threshold</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#BroodYear -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#BroodYear">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#YearBasis"/>
        <obo:IAO_0000115 xml:lang="en">The year that a cohort of salmon spawns.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Adapted from the Government of Canada Pacific Salmon Glossary</obo:IAO_0000119>
        <terms:source rdf:resource="https://www.pac.dfo-mpo.gc.ca/fm-gp/salmon-saumon/gloss-eng.html"/>
        <rdfs:comment xml:lang="en">The parental year for a group of returning salmon, i.e. the calendar year when the majority of parents of these fish spawned.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Brood year</rdfs:label>
        <skos:exactMatch rdf:resource="http://purl.dataone.org/odo/SALMON_00000520"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#CatchYear -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#CatchYear">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#YearBasis"/>
        <obo:IAO_0000115 xml:lang="en">The calendar year in which returning salmon are caught in fisheries (as opposed to the brood year in which the cohort was spawned).</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Catch year</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#ConfidenceCategory -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#ConfidenceCategory"/>
    


    <!-- https://w3id.org/gcdfo/salmon#ConservationUnit -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#ConservationUnit">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ReportingOrManagementStratum"/>
        <obo:IAO_0000115 xml:lang="en">A group of fish sufficiently isolated from other groups that, if extirpated, is very unlikely to recolonize naturally within an acceptable timeframe.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Canada&apos;s Policy for Conservation of Wild Pacific Salmon Glossary.</obo:IAO_0000119>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. (2005). Canada&apos;s Policy for Conservation of Wild Pacific Salmon. Retrieved from https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/315577.pdf.</obo:IAO_0000119>
        <terms:isPartOf rdf:resource="urn:ISBN:0662405382"/>
        <terms:source rdf:resource="https://www.pac.dfo-mpo.gc.ca/fm-gp/salmon-saumon/wsp-pss/policy-politique-eng.html#glossary"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Conservation Unit</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#Escapement -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#Escapement">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <obo:IAO_0000115 xml:lang="en">The process by which mature salmon pass through (or escape) fisheries and return to fresh water to spawn.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. (2005). Canada&apos;s Policy for Conservation of Wild Pacific Salmon. Retrieved from https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/315577.pdf.</obo:IAO_0000119>
        <terms:isPartOf rdf:resource="urn:ISBN:0662405382"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Escapement</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#EscapementMeasurement -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#EscapementMeasurement">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000109"/>
        <obo:IAO_0000115 xml:lang="en">A measurement or estimate of escapement tied to a stock and a survey event.</obo:IAO_0000115>
        <rdfs:comment xml:lang="en">Salmon escapement count</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Escapement measurement</rdfs:label>
        <skos:closeMatch rdf:resource="http://purl.dataone.org/odo/SALMON_00000479"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#EscapementSurveyEvent -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#EscapementSurveyEvent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#SurveyEvent"/>
        <obo:IAO_0000115 xml:lang="en">A survey event specifically for estimating escapement.</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Escapement Survey Event</rdfs:label>
        <skos:prefLabel xml:lang="en">Escapement Survey Event</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#ExploitationRate -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#ExploitationRate">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#TargetOrLimitRateOrAbundance"/>
        <obo:IAO_0000115 xml:lang="en">Expressed as a percentage, the proportion of the total return of adult salmon in a given year that die (harvested or removed) as a result of fishing activity. ER = total fishing mortality / (total fishing mortality + escapement)</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. (2019). Government of Canada. Pacific Salmon - Glossary. https://www.pac.dfo-mpo.gc.ca/fm-gp/salmon-saumon/gloss-eng.html. [Accessed 09-10-2025] </obo:IAO_0000119>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Exploitation rate</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#IndicatorRiver -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#IndicatorRiver">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ENVO_01000618"/>
        <obo:IAO_0000115 xml:lang="en">All spawning streams that are listed to be monitored by DFO are classified as an indicator or non-indicator stream. Indicator streams are those that have been identified in some Regions as providing more reliable indices of spawner abundance. Indicator streams tend to give relatively accurate annual spawner abundance estimates, given that they use higher quality methodologies and are more intensively/regularly surveyed. Indicator streams are also assumed to be representative of spawner returns to other streams in close geographic proximity. Non-indicator streams are typically less consistently surveyed, using more variable methodologies, and/or may simply be difficult to survey (e.g., poor water quality, remote location, etc.).</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Indicator river</rdfs:label>
        <skos:altLabel xml:lang="en">Indicator stream</skos:altLabel>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#LimitReferencePoint -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#LimitReferencePoint">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ReferencePoint"/>
        <obo:IAO_0000115 xml:lang="en">A fish stock experiencing serious harm may be depleted to the point where it has impaired productivity or reproductive capacity. As a result, the stock would be less resilient to fishing and could not easily rebuild. Serious harm may be irreversible, or only slowly reversible over the long-term, and can be due to fishing, other human-induced impacts, or natural causes. Under the Precautionary Approach policy and the Fish Stocks Provisions (FSPs), breaching the LRP is a trigger for a rebuilding plan.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. (2024). Introduction to Stock Assessment - Glossary. https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html. [Accessed 09-10-2025].</obo:IAO_0000119>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Limit reference point</rdfs:label>
        <skos:altLabel xml:lang="en">LRP</skos:altLabel>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#MetricBenchmark -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#MetricBenchmark">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000030"/>
        <obo:IAO_0000115 xml:lang="en">Benchmark value for a WSP metric that helps delineate status zones (for example, the limit reference point for absolute abundance).</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Pestal, G. et al. (2023). State of the Salmon: Rapid Status Assessment Approach for Pacific Salmon under Canada’s Wild Salmon Policy.</obo:IAO_0000119>
        <terms:source rdf:resource="https://publications.gc.ca/site/eng/9.928944/publication.html"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Metric benchmark</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000109"/>
        <obo:IAO_0000115 xml:lang="en">A measurement datum representing an empirically observed rate, count, or abundance derived from monitoring or survey data.</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Observed rate or abundance</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#ReferencePoint -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#ReferencePoint">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000030"/>
        <obo:IAO_0000115 xml:lang="en">Reference points are used as targets to achieve or as limits to avoid.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">DFO Introduction to Stock Assessment Glossary.</obo:IAO_0000119>
        <terms:source rdf:resource="https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Reference point</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#ReportingOrManagementStratum -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#ReportingOrManagementStratum">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000030"/>
        <obo:IAO_0000115 xml:lang="en">An information-defined grouping of fish populations used for reporting, conservation assessment, or management purposes. Examples include Conservation Units (CUs), Stock Management Units (MUs), and Designatable Units (DUs).</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Reporting or management stratum</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#Stock -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#Stock">
        <rdfs:subClassOf rdf:resource="http://rs.tdwg.org/dwc/terms/Organism"/>
        <obo:IAO_0000115 xml:lang="en">A biologically meaningful but operationally-focused salmon demographic unit used in assessment and reporting.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">DFO Introduction to Stock Assessment Glossary; Pacific Salmon Commission Technical Report No. 49.</obo:IAO_0000119>
        <terms:source rdf:resource="https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Stock</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.dataone.org/odo/SALMON_00000639"/>
        <skos:altLabel xml:lang="en">Fish stock type</skos:altLabel>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#StockAssessment -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#StockAssessment">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <obo:IAO_0000115 xml:lang="en">The scientific process of analyzing data to estimate the abundance and productivity of fish stocks in the past, present, and future. It is the backbone of successful science-based fisheries management.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. (2024). Introduction to Stock Assessment. Available at https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/module-1/09-eng.html [Accessed 10-28-2025].</obo:IAO_0000119>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Stock assessment</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#StockManagementUnit -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#StockManagementUnit">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ReportingOrManagementStratum"/>
        <obo:IAO_0000115 xml:lang="en">A collection of stocks that are managed together with a joint stock status.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">DFO Introduction to Stock Assessment Glossary.</obo:IAO_0000119>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. (2024). Introduction to Stock Assessment - Glossary. https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html. [Accessed 09-10-2025].</obo:IAO_0000119>
        <terms:source rdf:resource="https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Stock Management Unit</rdfs:label>
        <skos:altLabel xml:lang="en">SMU</skos:altLabel>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#SurveyEvent -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#SurveyEvent">
        <rdfs:subClassOf rdf:resource="http://rs.tdwg.org/dwc/terms/Event"/>
        <obo:IAO_0000115 xml:lang="en">A field event that collects information about key fish stocks to track their status, and track progress to implement fishery policies and measures for these stocks.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. (2021). About the Sustainability Survey for Fisheries. Available at https://www.dfo-mpo.gc.ca/reports-rapports/regs/sff-cpd/survey-sondage/about-propos-en.html. [Accessed 09-10-2025].</obo:IAO_0000119>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Survey event</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#TargetOrLimitRateOrAbundance -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#TargetOrLimitRateOrAbundance">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000109"/>
        <obo:IAO_0000115 xml:lang="en">A value specification representing a policy- or model-defined target or limit, such as TAC, UMSY, or reference points.</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Target or limit rate or abundance</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#TotalExploitationRate -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#TotalExploitationRate">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ExploitationRate"/>
        <obo:IAO_0000115 xml:lang="en">The overall proportion of the total return of adult salmon removed by all fisheries over a defined period, aggregating across gear or fishery components.</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Total exploitation rate</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#UpperStockReferencePoint -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#UpperStockReferencePoint">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ReferencePoint"/>
        <obo:IAO_0000115 xml:lang="en">A threshold below which fishing must be reduced to avoid reaching the LRP. The USR must be set at an appropriate distance above the LRP to provide sufficient opportunity for managers to recognize a declining stock status and sufficient time for management actions to take effect. </obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">DFO Introduction to Stock Assessment Glossary.</obo:IAO_0000119>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Upper stock reference</rdfs:label>
        <skos:altLabel xml:lang="en">USR</skos:altLabel>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#WSPBiologicalStatusZone -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#WSPBiologicalStatusZone"/>
    


    <!-- https://w3id.org/gcdfo/salmon#WSPRapidStatus -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#WSPRapidStatus">
        <obo:IAO_0000115 xml:lang="en">The WSP rapid status assessment approach builds on previously completed WSP integrated status assessments. The detailed WSP integrated assessments are labour intensive, taking 10-40 experts up to three days to complete for each group of CUs. For this reason, assessments have only been completed for ~11% of the 377 current Pacific Salmon CUs, many of which are outdated. The WSP rapid approach can fill gaps by expanding coverage of CU statuses across time, species, and geographic areas. The WSP rapid status approach ensures statuses are scientifically objective, consistent, and comparable across BC/Yukon CUs. This approach is also relatively easy to implement, applicable to data rich and data poor CUs, and can be updated annually.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Pestal, G., MacDonald, B. L., Grant, S. C. H., and Holt, C. A. (2023). State of the Salmon: rapid status assessment approach for Pacific salmon under Canada’s Wild Salmon Policy. Can. Tech. Rep. Fish. Aquat. Sci. 3570: xiv + 200 p.</obo:IAO_0000119>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">WSP rapid status</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#YearBasis -->


    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#YearBasis">
        <obo:IAO_0000115 xml:lang="en">A year-based temporal reference used to contextualize measurements or rates (e.g., brood year or catch year).</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Year basis</rdfs:label>
    </owl:Class>
    


