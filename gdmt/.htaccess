# This is the htaccess file for the w3id name for the Generic Dataset Metadata Template vocabulary

RewriteEngine on

# Rewrite for ontology id
RewriteRule ^$ https://bioportal.bioontology.org/ontologies/GDMT/?p=classes

# Rewrite for a term id
RewriteRule (.+) https://bioportal.bioontology.org/ontologies/GDMT/?p=classes&conceptid=https://w3id.org/gdmt/$1