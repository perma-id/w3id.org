RewriteEngine on

# Applications
RewriteRule ^specs/?(.*)$ https://specs.fairdigitalobjectframework.org/$1 [R=307,L]
RewriteRule ^ontology/metadata?(.*)$ https://github.com/utwente-scs/fdof-o/raw/main/owl/fdof-o-metadata.trig [R=307,L]
RewriteRule ^ontology#?(.*)$ https://github.com/utwente-scs/fdof-o/raw/main/owl/fdof-o.ttl [R=307,L]
RewriteRule ^types/metadata?(.*)$ https://github.com/utwente-scs/fdof-o/raw/main/owl/fdof-t-metadata.trig [R=307,L]
RewriteRule ^types#?(.*)$ https://github.com/utwente-scs/fdof-o/raw/main/owl/fdof-t.ttl [R=307,L]


# Paper FOIS 2023
RewriteRule ^fois23-paper/?(.*)$ https://github.com/utwente-scs/fdof-fois23-paper/$1 [R=307,L]
RewriteRule ^fois23-paper/ex1#?(.*) https://github.com/utwente-scs/fdof-fois23-paper/raw/main/amazonTop50Metadata.trig$1 [R=307,L]
RewriteRule ^fois23-paper/ex2#?(.*) https://github.com/utwente-scs/fdof-fois23-paper/raw/main/gufoMetadata.trig$1 [R=307,L]
RewriteRule ^fois23-paper/ex3#?(.*) https://github.com/utwente-scs/fdof-fois23-paper/raw/main/monkeySelfieMetadata.trig$1 [R=307,L]

# FDOF Examples
RewriteRule ^examples/fair-paper#?(.*) https://github.com/utwente-scs/fdof-o/raw/main/examples/fair-paper/fair-paper.trig$1 [R=307,L]
RewriteRule ^examples/?(.*) https://github.com/utwente-scs/fdof-o/raw/main/examples/$1 [R=307,L]
