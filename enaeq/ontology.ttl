              [ a       owl:Class ;
                owl:intersectionOf (EnergyAndResourceOntology:BuildingAutomationSystemResource [ a       owl:Restriction ;
                            owl:onProperty EnergyAndResourceOntology:hasFieldOfApplication ;
                            owl:someValuesFrom EnergyAndResourceOntology:ElectricalFieldOfApplication
                          ] [ a       owl:Restriction ;
                            owl:onProperty EnergyAndResourceOntology:hasFieldOfApplication ;
                            owl:someValuesFrom EnergyAndResourceOntology:TemperatureFieldOfApplication
                          ] [ a       owl:Restriction ;
                            owl:onProperty EnergyAndResourceOntology:hasFunctionality ;
                            owl:someValuesFrom EnergyAndResourceOntology:GroupFunctionality
                          ] [ a       owl:Restriction ;
                            owl:onProperty EnergyAndResourceOntology:hasFunctionality ;
                            owl:someValuesFrom EnergyAndResourceOntology:QueryFunctionality
                          ] [ a       owl:Restriction ;
                            owl:onProperty EnergyAndResourceOntology:hasFunctionality ;
                            owl:someValuesFrom EnergyAndResourceOntology:TemperatureMeasurementFunctionality
                          ] [ a       owl:Restriction ;
                            owl:onProperty EnergyAndResourceOntology:isSensorOf ;
                            owl:someValuesFrom
                                    [ a       owl:Class ;
                                      owl:unionOf (enaeq:EnergyEquipment EnergyAndResourceOntology:BuildingEnvironment)
                                    ]
                          ] [ a       owl:Restriction ;
                            owl:onClass EnergyAndResourceOntology:TemperatureState ;
                            owl:onProperty EnergyAndResourceOntology:hasState ;
                            owl:qualifiedCardinality
