Options +FollowSymLinks
RewriteEngine on

RewriteRule ^/imc/(.*) http://diyhistory.org/tmp/convert/index.php/$1 [R=302,L]
RewriteRule ^imc2/(.*) http://diyhistory.org/tmp/convert/index.php/$1 [R=302,L]
RewriteRule ^imc3(.*) http://diyhistory.org/tmp/convert/index.php/$1 [R=302,L]
RewriteRule ^/imc4(.*) http://diyhistory.org/tmp/convert/index.php/$1 [R=302,L]

RewriteCond "%{HTTP_ACCEPT}" "application/ld+json"
RewriteRule ^(.+) https://nakamura196.github.io/kouigenjimonogatari/$1.json [R=302,L]

RewriteRule ^(.*) https://nakamura196.github.io/kouigenjimonogatari/$1 [R=302,L]
