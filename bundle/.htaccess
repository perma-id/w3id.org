RewriteEngine on

RewriteRule ^$ https://researchobject.org/specifications/bundle/ [R=302,L]
RewriteRule ^draft(.*) https://researchobject.org/specifications/bundle/draft$1 [R=302,L]
RewriteRule ^(20\d\d)(.*) https://researchobject.org/specifications/bundle/$1$2 [R=302,L]
RewriteRule ^context$ https://researchobject.org/specifications/bundle/context.json [R=302,L]
RewriteRule ^context.json$ https://researchobject.org/specifications/bundle/context.json [R=302,L]
# Mainly for testing purposes - note github.io will now still redirect to https :(
RewriteRule ^context-http$ http://researchobject.org/specifications/bundle/context.json [R=302,L]
RewriteRule ^owl$ https://researchobject.org/specifications/bundle/bundle.owl [R=302,L]
