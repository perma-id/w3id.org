RewriteEngine On

# Fair Ease IDDAS homepage
RewriteRule ^$ https://fair-ease-iddas.maris.nl/ [R=302,L]

# Redirect to dataset.geodab.eu
RewriteRule ^easydata/(.+)$ https://dataset.geodab.eu/dataset/easydata/$1 [R=302,L]

RewriteRule ^eea-sdi/(.+)$ https://dataset.geodab.eu/dataset/UUID-456602db-4275-4410-8b68-436fd23ace69/$1 [R=302,L]

RewriteRule ^usnodcdbid/(.+)$ https://dataset.geodab.eu/dataset/usnodcdbid/$1 [R=302,L]

RewriteRule ^marineID/(.+)$ https://dataset.geodab.eu/dataset/marineID/$1 [R=302,L]

RewriteRule ^jrcdatacatalogdbid/(.+)$ https://dataset.geodab.eu/dataset/jrcdatacatalogdbid/$1 [R=302,L]

RewriteRule ^emodnet-network/(.+)$ https://dataset.geodab.eu/dataset/emodnet-network/$1 [R=302,L]

RewriteRule ^vito/(.+)$ https://dataset.geodab.eu/dataset/FROMREGISTRY--regprefseparator--registrytestid1--regprefseparator--dad0859e-206c-480b-a080-d4eea70ee22d/$1 [R=302,L]

RewriteRule ^wekeo/(.+)$ https://dataset.geodab.eu/dataset/wekeo/$1 [R=302,L]


# Redirect to data.blue-cloud.org (works with Query string parameters)
