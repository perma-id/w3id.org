Options +FollowSymLinks
# Turn off MultiViews
Options -MultiViews

# Rewrite engine setup
RewriteEngine On

#### Rewrite rules for w3id.org/demo-resources/demo-classifications
# text/turtle
RewriteCond %{HTTP:Accept} text/turtle [NC] 
RewriteRule ^(demo-classifications)$  https://jimjyang.github.io/showroom/xkos-ap-no/files/$1.ttl [R=302,L]
# Norwegian Bokmål
RewriteCond %{HTTP:Accept-Language} nb [NC]
RewriteRule ^(demo-classifications)$ https://jimjyang.github.io/showroom/xkos-ap-no/files/$1-nb [R=302,L]
# Norwegian Nynorsk
RewriteCond %{HTTP:Accept-Language} nn [NC]
RewriteRule ^(demo-classifications)$ https://jimjyang.github.io/showroom/xkos-ap-no/files/$1-nb [R=303,L]
# Norwegian Norsk
RewriteCond %{HTTP:Accept-Language} no [NC]
RewriteRule ^(demo-classifications)$ https://jimjyang.github.io/showroom/xkos-ap-no/files/$1-nb [R=303,L]
# English
RewriteCond %{HTTP:Accept-Language} en [NC]
RewriteRule ^(demo-classifications)$ https://jimjyang.github.io/showroom/xkos-ap-no/files/$1-en [R=302,L]
# English as default
RewriteRule ^(demo-classifications)$ https://jimjyang.github.io/showroom/xkos-ap-no/files/$1-en [R=303,L] 

