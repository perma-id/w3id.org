Header set Access-Control-Allow-Origin *
Header set Access-Control-Allow-Headers DNT,X-Mx-ReqToken,Keep-Alive,User-Agent,X-Requested-With,If-Modified$
Options +FollowSymLinks
RewriteEngine on
RewriteRule ^workshop/(.*) https://kg-construct.github.io/workshop/$1 [R=302,L]
RewriteRule ^workshop$ https://kg-construct.github.io/workshop [R=302,L]
RewriteRule ^costdkg-eswc-tutorial/(.*) https://kg-construct.github.io/eswc-dkg-tutorial-2022/$1 [R=302,L]
RewriteRule ^costdkg-eswc-tutorial$ https://kg-construct.github.io/eswc-dkg-tutorial-2022/ [R=302,L]
RewriteRule ^r2rml-implementation-report/(.*) https://rml.io/r2rml-implementation-report/$1 [R=302,L]
RewriteRule ^r2rml-implementation-report$ https://rml.io/r2rml-implementation-report [R=302,L]
RewriteRule ^rml-target-source/(.*) https://kg-construct.github.io/rml-target-source-spec/$1 [R=302,L]
RewriteRule ^rml-target-source$ https://kg-construct.github.io/rml-target-source-spec [R=302,L]
RewriteRule ^tpac/(.*) https://kg-construct.github.io/tpac-web/$1 [R=302,L]
RewriteRule ^tpac$ https://kg-construct.github.io/tpac-web [R=302,L]
RewriteRule ^rml-fno/(.*) https://kg-construct.github.io/rml-fno-spec/$1 [R=302,L]
RewriteRule ^rml-fno$ https://kg-construct.github.io/rml-fno-spec [R=302,L]
RewriteRule ^rml-star/(.*) https://kg-construct.github.io/rml-star-spec/$1 [R=302,L]
RewriteRule ^rml-star$ https://kg-construct.github.io/rml-star-spec [R=302,L]
RewriteRule ^survey/(.*) https://kg-construct.github.io/survey-website/$1 [R=302,L]
RewriteRule ^survey$ https://kg-construct.github.io/survey-website [R=302,L]
RewriteRule ^collections-containers$ https://kg-construct.github.io/collection-containers-spec [R=302,L]
RewriteRule ^(.*)$ http://w3.org/community/kg-construct [R=302,L]



