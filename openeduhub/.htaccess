RewriteEngine on
RewriteRule ^vocabs(/)?$ https://vocabs.openeduhub.de/$1 [R=302,L]
RewriteRule ^vocabs/(.*) https://vocabs.openeduhub.de/w3id.org/openeduhub/vocabs/$1 [R=302,L]
RewriteRule ^vocabs-playground(/)?$ https://openeduhub.github.io/oeh-metadata-vocabs-playground/$1 [R=302,L]
RewriteRule ^vocabs-playground/(.*) https://openeduhub.github.io/oeh-metadata-vocabs-playground/w3id.org/openeduhub/vocabs-playground/$1 [R=302,L]
RewriteRule ^terms(.*)? http://terms.openeduhub.de/oeh-terms.ttl [R=302,L]
RewriteRule ^(.*) https://wirlernenonline.de/$1 [R=302,L]
