Header set Access-Control-Allow-Origin *
Options +FollowSymLinks
RewriteEngine on

# Turn off MultiViews
Options -MultiViews

# current naming scheme
RewriteRule ^model$ https://raw.githubusercontent.com/researchstudio-sat/webofneeds/master/webofneeds/won-vocab/src/main/resources/ontology/won-model.ttl [R=301]
RewriteRule ^message$ https://raw.githubusercontent.com/researchstudio-sat/webofneeds/master/webofneeds/won-vocab/src/main/resources/ontology/won-message.ttl [R=301]
RewriteRule ^agreement$ https://raw.githubusercontent.com/researchstudio-sat/webofneeds/master/webofneeds/won-vocab/src/main/resources/ontology/won-agreement.ttl [R=301]
RewriteRule ^modification$ https://raw.githubusercontent.com/researchstudio-sat/webofneeds/master/webofneeds/won-vocab/src/main/resources/ontology/won-modification.ttl [R=301]

# New naming scheme for core ontology
RewriteRule ^core$ https://raw.githubusercontent.com/researchstudio-sat/webofneeds/master/webofneeds/won-vocab/src/main/resources/ontology/won-core.ttl [R=301]


# Rule allowing for adding small vocabularies that follow a naming pattern without having to adapt the redirects here
RewriteRule ^ext/([a-zA-Z0-9_\-]+)$ https://raw.githubusercontent.com/researchstudio-sat/webofneeds/master/webofneeds/won-vocab/src/main/resources/ontology/ext/won-ext-$1.ttl [R=301]
