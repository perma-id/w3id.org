Options +FollowSymLinks

# Directive to ensure *.rdf files served as appropriate content type,
# if not present in main apache config
# AddType application/rdf+xml .rdf
AddType text/turtle .ttl


RewriteEngine on

#w3id.org/quality/qskos
# set of rewrite providing stable URI for latest version of dump w3id.org/quality/qskos
#qskos
RewriteCond %{HTTP_ACCEPT} ^.*text/turtle.* 
RewriteRule ^qskos$ https://raw.githubusercontent.com/riccardoAlbertoni/qSKOS/fd388fde7fbc9e851e92599c736394946e00d5bf/src/main/resources/DQVMetricsDimensionForQSKOS.ttl  [R=303,NE,L]
#default response: ttl
RewriteRule ^qskos$ https://raw.githubusercontent.com/riccardoAlbertoni/qSKOS/fd388fde7fbc9e851e92599c736394946e00d5bf/src/main/resources/DQVMetricsDimensionForQSKOS.ttl  [R=303,NE,L]

#set of rewrite to get a specific qskos entities
RewriteRule ^qskos/(.*)$ https://raw.githubusercontent.com/riccardoAlbertoni/qSKOS/fd388fde7fbc9e851e92599c736394946e00d5bf/src/main/resources/DQVMetricsDimensionForQSKOS.ttl#$1 [R=303,NE,L]