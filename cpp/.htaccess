# Redirects for the CPP mechanisms dataset
Options +FollowSymLinks
RewriteEngine On

# Dataset RDF files
RewriteRule ^dataset/mechanisms\.ttl$ https://mariacastillo982.github.io/cpp-mechanisms/data/mechanisms.ttl [R=302,L]
RewriteRule ^dataset/mechanisms\.jsonld$ https://mariacastillo982.github.io/cpp-mechanisms/data/mechanisms.jsonld [R=302,L]

# Default landing page (README or docs/index.html)
RewriteRule ^dataset/mechanisms$ https://mariacastillo982.github.io/cpp-mechanisms/ [R=302,L]

# Vocabulary/schema
RewriteRule ^schema(.*)$ https://mariacastillo982.github.io/cpp-mechanisms/schema$1 [R=302,L]
