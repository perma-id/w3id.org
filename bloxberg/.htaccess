Header set Access-Control-Allow-Origin *
Header set Access-Control-Allow-Headers DNT,X-Mx-ReqToken,Keep-Alive,User-Agent,X-Requested-With,If-Modified$
Options +FollowSymLinks
RewriteEngine on
RewriteRule ^schema/research_object_certificate_v1$ https://certify.bloxberg.org/research_object_certificate_v1 [R=301,L,NC]
RewriteRule ^$ https://www.bloxberg.org/ [R=302,L]
RewriteRule ^research_object_certificate_v1$ https://certify.bloxberg.org/research_object_certificate_v1 [R=302,L]
RewriteRule ^(.*)$ https://bloxberg.org/ [R=302,L]
