RewriteEngine on

RewriteRule ^$ https://docs.ai4os.eu/projects/ai4-metadata/en/latest/ [R=302,L]

# Comment this until we have versions

#RewriteRule ^(0|[1-9]\d*)\.(0|[1-9]\d*)\.(0|[1-9]\d*)(?:-((?:0|[1-9]\d*|\d*[a-zA-Z-][0-9a-zA-Z-]*)(?:\.(?:0|[1-9]\d*|\d*[a-zA-Z-][0-9a-zA-Z-]*))*))?(?:\+([0-9a-zA-Z-]+(?:\.[0-9a-zA-Z-]+)*))?$ https://docs.ai4os.eu/projects/ai4-metadata/en/$1.$2.$3/ [R=302,L]

# Lets point only to latest for the time being. We can add versioning afterwards
RewriteRule ^/vocabulary/category/(.+)$  https://docs.ai4os.eu/projects/ai4-metadata/en/latest/reference/category.html#$1 [R=302,L]
RewriteRule ^/vocabulary/library/(.+)$   https://docs.ai4os.eu/projects/ai4-metadata/en/latest/reference/library.html#$1 [R=302,L]
RewriteRule ^/vocabulary/tasktype/(.+)$  https://docs.ai4os.eu/projects/ai4-metadata/en/latest/reference/tasktype.html#$1 [R=302,L]
