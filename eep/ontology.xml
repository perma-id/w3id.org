        <rdfs:isDefinedBy rdf:resource="https://w3id.org/eep/"/>
        <rdfs:label xml:lang="en">usedProcedure</rdfs:label>
        <ns1:term_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">stable</ns1:term_status>
    </owl:ObjectProperty>
    


