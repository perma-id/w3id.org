RewriteCond %{HTTP_ACCEPT} application/ld\+json
RewriteRule ^(.*)$ https://w3c.github.io/dpv/$1.ldjson [R=302,L]

# Default
# w3id.org/dpv
RewriteRule ^$ https://w3c.github.io/dpv/dpv [R=302,L]
# w3id.org/dpv/*
RewriteRule ^(.*)$ https://w3c.github.io/dpv/$1 [R=302,L]

