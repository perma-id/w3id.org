# ============================================

# AEROSPACE-X Content Negotiation

# FÃ¼r .htaccess optimiert

# ============================================

Header set Access-Control-Allow-Origin "*"

# Content-Type Definitions

AddType application/rdf+xml .rdf .owl
AddType text/turtle .ttl
AddType text/n3 .n3
AddType application/n-triples .nt
AddType application/ld+json .json
AddType application/yaml .yaml
AddType application/schema+json .schema.json
AddType application/asset-administration-shell-package .aasx

Options +FollowSymLinks
RewriteEngine on

# ============================================

# 1. ODRL Verifiable Credential Profile

# ============================================

RewriteRule ^ovc(/.*)?$ https://gitlab.com/gaia-x/lab/policy-reasoning/odrl-vc-profile/-/raw/main/ovc-1.ttl [R=303,L]

# ============================================

# 2. Ontology Content Negotiation

# ============================================

# Turtle (SHACL shapes)

RewriteCond %{HTTP_ACCEPT} text/turtle [NC,OR]
RewriteCond %{HTTP_ACCEPT} application/x-turtle [NC]
RewriteRule ^ontology/(development|[0-9.]+)?/?$ https://a-x.pages.fraunhofer.de/ontology/ontology.shacl.ttl [R=303,L]

# RDF/XML (OWL)

RewriteCond %{HTTP_ACCEPT} application/rdf\+xml [NC]
RewriteRule ^ontology/(development|[0-9.]+)?/?$ https://a-x.pages.fraunhofer.de/ontology/ontology.owl.ttl [R=303,L]

# ============================================

# 3. Data Product Models - Content Negotiation

# Pattern: /aerospace-x/data-product-model/{model}/{version}

