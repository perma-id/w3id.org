RewriteEngine on
RewriteCond %{HTTP_ACCEPT} !application/rdf\+xml.*(text/html|application/xhtml\+xml|text/\*|\*/\*)
RewriteCond %{HTTP_ACCEPT} text/html [OR]
RewriteCond %{HTTP_ACCEPT} application/xhtml\+xml [OR]
RewriteCond %{HTTP_ACCEPT} text/\* [OR]
RewriteCond %{HTTP_ACCEPT} \*/\* [OR]
RewriteCond %{HTTP_USER_AGENT} ^Mozilla/.*
RewriteRule ^$ https://cpf-relations.sourceforge.io/index.html  [R=303,NE,L]
RewriteCond %{HTTP_ACCEPT} ^.*application/rdf+xml.*

RewriteRule ^$ https://sourceforge.net/p/cpf-relations/code/ci/master/tree/cpf-relationships-vc.owl [R=303,NE,L]
RewriteRule ^$ https://sourceforge.net/p/cpf-relations/code/ci/master/tree/cpf-relationships-vc.owl [R=303,NE,L]Options +FollowSymLinks
