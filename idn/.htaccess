Options +FollowSymLinks
RewriteEngine on


# Website Homepage
RewriteRule ^/?$ https://idnau.org/ [R=302,L]


# PIDs

# Catalogued Resources
# Register
RewriteRule ^resources/$ http://idn.kurrawong.net/catalog/idndc [R=302,L]
# Individual Resources
RewriteRule ^resources/(.*)$ http://idn.kurrawong.net/catalog/idndc/$1 [R=302,L]

# Datasets
RewriteRule ^datasets/AGIL http://idn.kurrawong.net/dataset/AGIL [R=302,L]
RewriteRule ^datasets/NNTT http://idn.kurrawong.net/dataset/NNTT [R=302,L]

# Vocabularies
# Register
RewriteRule ^vocabs/$ http://idn.kurrawong.net/vocab [R=302,L]
# Individual Resources
RewriteRule ^vocabs/(.*)$ http://idn.kurrawong.net/vocab/$1 [R=302,L]
