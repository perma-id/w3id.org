RewriteEngine On

# Rule 1: Redirect base to index
RewriteRule ^$ https://skz.github.io/skz-public-resources/index.html [R=301,L]

# Rule 2: Redirect /resource to a specific page, with or without a fragment
RewriteRule ^(public/)?ontology(|#(.*))$ https://skz.github.io/skz-public-resources/skz.html$2 [R=301,L]
