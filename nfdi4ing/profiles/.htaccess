Header set Access-Control-Allow-Origin *
Options +FollowSymLinks
RewriteEngine on

# HTML:
# RewriteCond %{HTTP_ACCEPT} text/html
RewriteCond %{REQUEST_URI} ^(\/nfdi4ing\/profiles\/)$
RewriteRule ^(.*)$ https://demo.aims-projekt.de [R=303,L,NE]

# Redirec profiles to editor
RewriteCond %{REQUEST_URI} (.*)
RewriteRule ^(.*)$ https://demo.aims-projekt.de/#/editor?applicationProfile=https://w3id.org/$1 [R=303,L,NE]
