RewriteEngine on

# Redirection Match rule for accessing the catalog repository git page

RedirectMatch 302 ^/?$ https://github.com/unibz-core/ontouml-models
RedirectMatch 302 ^/git/?$ https://github.com/unibz-core/ontouml-models

# Redirection Match rule for accessing the catalog datasets

RedirectMatch 302 ^/dataset/(.*)$ https://github.com/unibz-core/ontouml-models/tree/master/$1

# Redirection Match rule for accessing every resource in the main folder of the catalog

RedirectMatch 302 ^/catalog/(.*)$ https://raw.githubusercontent.com/unibz-core/ontouml-models/master/$1