Header set Access-Control-Allow-Origin *
Options +FollowSymLinks
RewriteEngine on

RewriteRule ^(.*)/fourpin/index.html$ https://github.com/rjlop/fourpin/docs/index.html [R=302,L]

RewriteRule ^(.*)/fourpin/ontology$ https://github.com/rjlop/fourpin/ontology/index.owl [R=302,L]

RewriteRule ^(.*)/fourpin/(.*)$ https://github.com/rjlop/fourpin/$2 [R=302,L]
