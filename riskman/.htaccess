Header set Access-Control-Allow-Origin *
Options +FollowSymLinks
RewriteEngine on

RewriteRule ^$ https://cl-tud.github.io/riskman-documentation/ [R=301,L]

RewriteRule ^ontology/?$ https://w3id.org/riskman/ontology/1.0.0 [R=303,L]
RewriteRule ^ontology/1.0.0/?$ https://raw.githubusercontent.com/cl-tud/riskman/main/ontology-1.0.0.ttl [R=303,L]

RewriteRule ^ontology/security/?$ https://w3id.org/riskman/ontology/security/1.0.0 [R=303,L]
RewriteRule ^ontology/security/1.0.0/?$ https://raw.githubusercontent.com/cl-tud/riskman/main/ontology-security-1.0.0.ttl [R=303,L]

RewriteRule ^shapes/?$ https://w3id.org/riskman/shapes/1.0.0 [R=303,L]
RewriteRule ^shapes/1.0.0/?$ https://raw.githubusercontent.com/cl-tud/riskman/main/shapes-1.0.0.ttl [R=303,L]

RewriteRule ^shapes/security/?$ https://w3id.org/riskman/shapes/security/1.0.0 [R=303,L]
RewriteRule ^shapes/security/1.0.0/?$ https://raw.githubusercontent.com/cl-tud/riskman/main/shapes-security-1.0.0.ttl [R=303,L]

RewriteRule ^repo/?$ https://github.com/cl-tud/riskman/ [R=303,L]
