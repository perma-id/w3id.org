Header set Access-Control-Allow-Origin *
Options +FollowSymLinks
RewriteEngine on

RewriteRule ^$ https://www.go-fair.org/ [R=302,L]

RewriteRule ^maturity_indicator/?$ https://github.com/FAIRMetrics/Metrics [R=302,L]
RewriteRule ^maturity_indicator/terms/([^/]+)/?$ https://github.com/FAIRMetrics/Metrics/blob/master/MaturityIndicators/$1 [R=302,L]
RewriteRule ^maturity_indicator/terms/([^/]+)/([^/]+)$ https://github.com/FAIRMetrics/Metrics/blob/master/MaturityIndicators/$1/$2 [R=302,L]
RewriteRule ^maturity_indicator/np/([^/]+)/([^/]+)/.*$ https://github.com/FAIRMetrics/Metrics/blob/master/MaturityIndicators/$1/$2 [R=302,L]

RewriteRule ^icc/(.*)$ https://www.go-fair.org/implementation-networks/#$1 [R=302,NE,L]


RewriteCond %{HTTP_ACCEPT} text/html
RewriteRule ^principles/terms/([^/]+)$ https://peta-pico.github.io/FAIR-nanopubs/principles/index-en.html#https://w3id.org/fair/principles/terms/$1 [R=302,L]
