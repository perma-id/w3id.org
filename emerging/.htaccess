Header set Access-Control-Allow-Origin *
Header set Access-Control-Allow-Headers DNT,X-Mx-ReqToken,Keep-Alive,User-Agent,X-Requested-With,If-Modified$
Options +FollowSymLinks
RewriteEngine on

# emerging ns(namespace) mappings, folders for json files and file mappings for jsonld
# file mappings:
RewriteRule ^vocab/v1(.*)$ https://ixofoundation.github.io/ns/vocab/v1/index.jsonld$1 [R=302,L]
RewriteRule ^ns/vocab/v1(.*)$ https://ixofoundation.github.io/ns/vocab/v1/index.jsonld$1 [R=302,L]
RewriteRule ^artefacts/cleanCookingClaim(.*)$ https://ixofoundation.github.io/ns/artefacts/cleanCookingClaim.jsonld$1 [R=302,L]
RewriteRule ^ns/artefacts/cleanCookingClaim(.*)$ https://ixofoundation.github.io/ns/artefacts/cleanCookingClaim.jsonld$1 [R=302,L]
RewriteRule ^artefacts/deviceCredential(.*)$ https://ixofoundation.github.io/ns/artefacts/deviceCredential.jsonld$1 [R=302,L]
RewriteRule ^ns/artefacts/deviceCredential(.*)$ https://ixofoundation.github.io/ns/artefacts/deviceCredential.jsonld$1 [R=302,L]

# folder mappings:
RewriteRule ^attributes(.*)$ https://ixofoundation.github.io/ns/attributes$1 [R=302,L]
RewriteRule ^claim-schemas(.*)$ https://ixofoundation.github.io/ns/claim-schemas$1 [R=302,L]
RewriteRule ^credential-schemas(.*)$ https://ixofoundation.github.io/ns/credential-schemas$1 [R=302,L]

# https://w3id.org/emerging -> https://emerging-eco.github.io
RewriteRule ^(.*)$ https://emerging-eco.github.io/$1 [R=302,L]
