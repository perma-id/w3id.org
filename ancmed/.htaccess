RewriteEngine on

# Urls ending in numbers redirect to the relevant php page for the entry
RewriteRule ^(.*)\/([0-9]+)$ https://ancmed.ulb.be/details.php?row=$2 [R=302,L]
RewriteRule ^(.*)\/([0-9]+)/$ https://ancmed.ulb.be/details.php?row=$2 [R=302,L]

# Default
RewriteRule ^(.*)$ https://ancmed.ulb.be [R=302,L]
