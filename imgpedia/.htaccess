Header set Access-Control-Allow-Origin *
Header set Access-Control-Allow-Headers DNT,X-Mx-ReqToken,Keep-Alive,User-Agent,X-Requested-With,If-Modified$
Options +FollowSymLinks
RewriteEngine on
RewriteRule ^$ http://imgpedia.dcc.uchile.cl [R=302,L]
RewriteRule ^sparql$ http://imgpedia.dcc.uchile.cl/sparql [R=302,L]
RewriteRule ^ontology$ http://imgpedia.dcc.uchile.cl/ontology [R=302,L]