                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <https://sargon-n5geh.netlify.app/ontology/1.0/data_properties/logging_interval> ;
                          owl:someValuesFrom xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
