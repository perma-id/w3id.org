            rdfs:domain <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/gbBuildingOntology.owl#Zone> ;
            rdfs:range :ExtWall ;
            rdfs:comment "Relation linking building Zone and its exterior wall"@en ;
            rdfs:label "hasExteriorWall"@en .


###  https://w3id.org/ee4i#hasExtWindow
:hasExtWindow rdf:type owl:ObjectProperty ;
              owl:inverseOf :isExtWindowOf ;
              rdf:type owl:InverseFunctionalProperty ;
              rdfs:domain <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/gbBuildingOntology.owl#Zone> ;
              rdfs:range :ExtWindow ;
              rdfs:comment "Relation linking building Zone and its exterior window"@en ;
              rdfs:label "hasExteriorWindow"@en .


###  https://w3id.org/ee4i#hasForecast
:hasForecast rdf:type owl:ObjectProperty ;
             owl:inverseOf :isForecastOf ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :EnergyProduction
                                         <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergyDemand>
                                         <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/gbBuildingOntology.owl#Weather>
