                                                                   owl:someValuesFrom :UniversalConstant
                                                                 ] ,
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :containsVariable ;
                                                                   owl:someValuesFrom <http://identifiers.org/mamo/Variable>
                                                                 ] ,
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :isMathematicalModelOf ;
                                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                   owl:onClass :KPI
                                                                 ] ,
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/gbBuildingOntology.owl#containsEquation> ;
                                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                   owl:onClass :Equation
                                                                 ] ;
                                                 rdfs:comment "The mathematical model for KPI calculation. The model is composed of a set of variables and a set of equations that establish relationships between the variables. "@en ;
