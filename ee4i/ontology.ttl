###  https://w3id.org/ee4i#PowerDeliveryUnit
:PowerDeliveryUnit rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( :PowerEquipment
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :deliversEnergy ;
                                                                owl:someValuesFrom :EnergySupply
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ,
                                       [ owl:intersectionOf ( :PowerEquipment
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :deliversEnergy ;
                                                                owl:someValuesFrom <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergySupply>
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty :deliversEnergyType ;
