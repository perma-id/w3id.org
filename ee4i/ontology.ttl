###  https://w3id.org/ee4i#KPI
:KPI rdf:type owl:Class ;
     rdfs:subClassOf [ rdf:type owl:Restriction ;
                       owl:onProperty :hasCalculation ;
                       owl:someValuesFrom :KPICalculation
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty <http://purl.org/dc/terms/identifier> ;
                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                       owl:onDataRange xsd:string
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasKPIDefinition ;
                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                       owl:onDataRange xsd:string
                     ] ;
     rdfs:comment "Key Performance Indicator that measures the progress towards stakeholder's key performance goal" ;
     rdfs:label "KeyPerformanceIndicator" .


###  https://w3id.org/ee4i#KPICalculation
:KPICalculation rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://purl.oclc.org/NET/ssnx/ssn#hasInput> ;
                                  owl:someValuesFrom :DatumSource
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasCalculationComponent ;
                                  owl:someValuesFrom :KPICalculationComponent
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://purl.oclc.org/NET/ssnx/ssn#hasOutput> ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :KPIValue
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasAssociatedObject ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :KPIEvaluatedObject
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasCalculationModel ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass <http://identifiers.org/mamo/Mathematical_model>
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasEvaluationTimeStep ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass <http://www.w3.org/2006/time#Interval>
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :isCalculationOf ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :KPI
                                ] ;
                rdfs:comment "The KPI calculation to calculate specific KPI with mathematical model and datum source"@en ;
                rdfs:label "KPICalculation"@en .


