Header set Access-Control-Allow-Origin *
Header set Access-Control-Allow-Headers DNT,X-Mx-ReqToken,Keep-Alive,User-Agent,X-Requested-With,If-Modified$
Options +FollowSymLinks
RewriteEngine on
RewriteRule ^$ https://rml.io/yarrrml/ [R=302,L]
RewriteRule ^spec/?$ https://rml.io/yarrrml/spec/ [R=302,L]
RewriteRule ^tutorial/?$ https://rml.io/yarrrml/tutorial/getting-started/ [R=302,L]
RewriteRule ^tutorial/getting-started/?$ https://rml.io/yarrrml/tutorial/getting-started/ [R=302,L]
RewriteRule ^matey/screencast-target/?$ https://rml.io/yarrrml/matey/screencast-target.html [R=302,L]
RewriteRule ^matey/screencast/?$ https://rml.io/yarrrml/matey/screencast.html [R=302,L]
RewriteRule ^matey/repository/?$ https://github.com/RMLio/matey [R=302,L]
RewriteRule ^matey/?$ https://rml.io/yarrrml/matey/ [R=302,L]
RewriteRule ^(.*)$ https://rml.io/$1 [R=302,L]
