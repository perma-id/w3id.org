Header set Access-Control-Allow-Origin *
Options +FollowSymLinks
RewriteEngine on

RewriteCond %{REQUEST_METHOD} GET
RewriteRule ^nanopub-query-1.1/(.*)$ https://query.np.trustyuri.net/$1 [R=301,L]
RewriteCond %{REQUEST_METHOD} POST
RewriteRule ^nanopub-query-1.1/(.*)$ https://query.np.trustyuri.net/$1 [R=308,L]

RewriteCond %{REQUEST_METHOD} GET
RewriteRule ^nanodash-2.x/(.*)$ https://nanodash.knowledgepixels.com/$1 [R=301,L]
RewriteCond %{REQUEST_METHOD} POST
RewriteRule ^nanodash-2.x/(.*)$ https://nanodash.knowledgepixels.com/$1 [R=308,L]
