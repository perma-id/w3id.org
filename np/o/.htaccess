Header set Access-Control-Allow-Origin *
Options +FollowSymLinks
RewriteEngine on

RewriteRule ^$ https://github.com/peta-pico/npo [R=302,L]

RewriteCond %{HTTP_ACCEPT} text/html
RewriteRule ^ntemplate/([^/]+)$ https://peta-pico.github.io/npo/ntemplate/index-en.html#https://w3id.org/np/o/ntemplate/$1 [R=302,L,NE]



RewriteRule ^ntemplate/([^/]+)$ https://w3id.org/np/o/ntemplate/latest/$1 [R=302,L]
RewriteRule ^ntemplate/np/.+/(RA[A-Za-z0-9_\-]{43})$ http://purl.org/np/$1 [R=302,L]

RewriteRule ^ntemplate/latest/NanopubTemplateOntology$ http://purl.org/np/RA6-aP9t3OcdluJQZ20TvndmU-7YbecuPdUDNBJbdLbHA [R=302,L]
RewriteRule ^ntemplate/latest/AssertionTemplate$ http://purl.org/np/RAgnTswSJVbteT-6nqKlPqk-cpPjG6z-slHpxh4r5R6uA [R=302,L]
