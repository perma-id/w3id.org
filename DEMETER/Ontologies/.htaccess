Options +FollowSymLinks
# Turn off MultiViews
Options -MultiViews

# Directive to ensure *.rdf files served as appropriate content type,
# if not present in main apache config
AddType application/rdf+xml .rdf
AddType application/rdf+xml .owl
AddType text/turtle .ttl

RewriteEngine on

#Rewrite rules for Asteroids vocabulary extension 
RewriteCond %{HTTP_ACCEPT} !application/rdf\+xml.*(text/html|application/xhtml\+xml|text/\*|\*/\*)
RewriteCond %{HTTP_ACCEPT} text/html [OR]
RewriteCond %{HTTP_ACCEPT} application/xhtml\+xml [OR]
RewriteCond %{HTTP_ACCEPT} text/\* [OR]
RewriteCond %{HTTP_ACCEPT} \*/\* [OR]
RewriteCond %{HTTP_USER_AGENT} ^Mozilla/.*

RewriteRule ^agentparty$ https://github.com/gero84/DEMETER/blob/master/agentparty.owl [R=303,NE,L]
RewriteCond %{HTTP_ACCEPT} ^.*text/turtle.* 
#default response: owl
RewriteRule ^agentparty$ https://github.com/gero84/DEMETER/blob/master/agentparty.owl [R=303,NE,L]

RewriteRule ^agriproduct$ https://github.com/gero84/DEMETER/blob/master/agriproduct.owl [R=303,NE,L]
RewriteCond %{HTTP_ACCEPT} ^.*text/turtle.* 
#default response: owl
RewriteRule ^agriproduct$ https://github.com/gero84/DEMETER/blob/master/agriproduct.owl [R=303,NE,L]

RewriteRule ^animal$ https://github.com/gero84/DEMETER/blob/master/animal.owl[R=303,NE,L]
RewriteCond %{HTTP_ACCEPT} ^.*text/turtle.* 
#default response: owl
RewriteRule ^animal$ https://github.com/gero84/DEMETER/blob/master/animal.owl [R=303,NE,L]

RewriteRule ^pest$ https://github.com/gero84/DEMETER/blob/master/pest.owl[R=303,NE,L]
RewriteCond %{HTTP_ACCEPT} ^.*text/turtle.* 
#default response: owl
RewriteRule ^pest$ https://github.com/gero84/DEMETER/blob/master/pest.owl [R=303,NE,L]

RewriteRule ^properties$ https://github.com/gero84/DEMETER/blob/master/properties.owl[R=303,NE,L]
RewriteCond %{HTTP_ACCEPT} ^.*text/turtle.* 
#default response: owl
RewriteRule ^properties$ https://github.com/gero84/DEMETER/blob/master/properties.owl [R=303,NE,L]
