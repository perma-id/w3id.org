RewriteEngine On

# defined terms
RewriteRule ^def/berichtsreihe$            https://reportdcat-ap.de/#klasse-berichtsreihe           [R=302,NE,L]
RewriteRule ^def/bericht$                  https://reportdcat-ap.de/#klasse-bericht                 [R=302,NE,L]
RewriteRule ^def/datenressource$           https://reportdcat-ap.de/#klasse-datenressource          [R=302,NE,L]
RewriteRule ^def/it-verfahren$             https://reportdcat-ap.de/#klasse-it-verfahren            [R=302,NE,L]
RewriteRule ^def/fachkennzahl$             https://reportdcat-ap.de/#klasse-fachkennzahl            [R=302,NE,L]
RewriteRule ^def/abstrakte-fachkennzahl$   https://reportdcat-ap.de/#klasse-abstrakte-fachkennzahl  [R=302,NE,L]
RewriteRule ^def/analyseumgebung$          https://reportdcat-ap.de/#sd-analyseumgebung             [R=302,NE,L]
RewriteRule ^def/erhebungsgrund$           https://reportdcat-ap.de/#sd-erhebungsgrund              [R=302,NE,L]
RewriteRule ^def/rechtsgrundlage$          https://reportdcat-ap.de/#sd-rechtsgrundlage             [R=302,NE,L]

# controlled vocabularies
RewriteRule ^def/datendomaene-fhh(/.*)?$   https://reportdcat-ap.de/#kv-datendomaene-fhh            [R=302,NE,L]
RewriteRule ^def/id-schema(/.*)?$          https://reportdcat-ap.de/#kv-id-schema                   [R=302,NE,L]

# catch all
RewriteRule ^def/?$ https://reportdcat-ap.de/ [R=302,L]
RewriteRule ^/?$    https://reportdcat-ap.de/ [R=302,L]