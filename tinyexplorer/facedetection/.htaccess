Header set Access-Control-Allow-Origin *
Header set Access-Control-Allow-Headers DNT,X-Mx-ReqToken,Keep-Alive,User-Agent,X-Requested-With,If-Modified-Since,Cache-Control,Content-Type,Accept-Encoding
Options +FollowSymLinks
RewriteEngine on

# Redirect all requests to the GitHub Pages documentation
RewriteRule ^(.*)$ https://cardiff-babylab.github.io/tinyexplorer-facedetectionapp/$1 [R=302,L]