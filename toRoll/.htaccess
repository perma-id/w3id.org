RewriteEngine On

# Redirect to toRoll instance
RewriteRule ^$ https://skosmos.toroll.datamanager.kit.edu/ [R=303,L,QSA]

#Redirect to toRoll Skosmos dashboard
RewriteRule   ^vocs/?$	https://skosmos.toroll.datamanager.kit.edu/ [R=303,L,QSA]

# Redirect to Tagin & Otijjot Meshunnot vocabulary
RewriteRule   ^vocs/taginOM/?$	https://skosmos.toroll.datamanager.kit.edu/TaginOtijjotMeshunnot-1/ [R=303,L,QSA]

# Redirect individual concept of Tagin & OM
RewriteRule   ^vocs/taginOM/([^/]+)$ https://skosmos.toroll.datamanager.kit.edu/TaginOtijjotMeshunnot-1/en/page/$1 [R=303,L,QSA]
