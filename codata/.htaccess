Options +FollowSymLinks
RewriteEngine on
RewriteRule ^model/(.*)$ https://api.codata.org/drum/model/$1 [R=303,L]
RewriteRule ^concepts/(.*)$ https://api.codata.org/drum/concepts/$1 [R=303,L]
RewriteRule ^constants/(.*)$ https://api.codata.org/drum/constants/$1 [R=303,L]
RewriteRule ^quantities/(.*)$ https://api.codata.org/drum/quantities/$1 [R=303,L]
RewriteRule ^units/(.*)$ https://api.codata.org/drum/units/$1 [R=303,L]
RewriteRule ^versions/(.*)$ https://api.codata.org/drum/versions/$1 [R=303,L]
RewriteRule ^([\S]*)$ https://codata.org/$1 [R=303,L]
