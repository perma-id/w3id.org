    <!-- http://www.semanticdesktop.org/ontologies/2007/01/19/nie#depends -->

    <owl:ObjectProperty rdf:about="&nie;depends">
        <rdfs:label>depends</rdfs:label>
        <rdfs:comment>Dependency relation. A piece of content depends on another piece of data in order to be properly understood/used/interpreted.</rdfs:comment>
        <rdfs:range rdf:resource="&nie;DataObject"/>
        <rdfs:subPropertyOf rdf:resource="&nie;relatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/01/19/nie#hasLogicalPart -->

    <owl:ObjectProperty rdf:about="&nie;hasLogicalPart">
        <rdfs:label>hasLogicalPart</rdfs:label>
        <rdfs:comment>Generic property used to express &apos;logical&apos; containment relationships between InformationElements. NIE extensions are encouraged to provide more specific subproperties of this one. It is advisable for actual instances of InformationElement to use those specific subproperties. Note the difference between &apos;physical&apos; containment (hasPart) and logical containment (hasLogicalPart)</rdfs:comment>
        <rdfs:domain rdf:resource="&nie;InformationElement"/>
        <rdfs:range rdf:resource="&nie;InformationElement"/>
        <nrl:inverseProperty rdf:resource="&nie;isLogicalPartOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/01/19/nie#hasPart -->

    <owl:ObjectProperty rdf:about="&nie;hasPart">
        <rdfs:range rdf:resource="&nie;DataObject"/>
        <rdfs:domain rdf:resource="&nie;InformationElement"/>
        <rdfs:subPropertyOf rdf:resource="&nie;relatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/01/19/nie#interpretedAs -->

    <owl:ObjectProperty rdf:about="&nie;interpretedAs">
        <rdfs:label>interpretedAs</rdfs:label>
        <rdfs:comment>Links the DataObject with the InformationElement it is interpreted as.</rdfs:comment>
        <rdfs:domain rdf:resource="&nie;DataObject"/>
        <rdfs:range rdf:resource="&nie;InformationElement"/>
        <nrl:inverseProperty rdf:resource="&nie;isStoredAs"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/01/19/nie#isLogicalPartOf -->

    <owl:ObjectProperty rdf:about="&nie;isLogicalPartOf">
        <rdfs:range rdf:resource="&nie;InformationElement"/>
        <rdfs:domain rdf:resource="&nie;InformationElement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/01/19/nie#isPartOf -->

    <owl:ObjectProperty rdf:about="&nie;isPartOf">
        <rdfs:domain rdf:resource="&nie;DataObject"/>
        <rdfs:range rdf:resource="&nie;InformationElement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/01/19/nie#isStoredAs -->

    <owl:ObjectProperty rdf:about="&nie;isStoredAs">
        <rdfs:label>isStoredAs</rdfs:label>
        <rdfs:comment>Links the information element with the DataObject it is stored in.</rdfs:comment>
        <rdfs:range rdf:resource="&nie;DataObject"/>
        <rdfs:domain rdf:resource="&nie;InformationElement"/>
        <nrl:inverseProperty rdf:resource="&nie;interpretedAs"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/01/19/nie#links -->

    <owl:ObjectProperty rdf:about="&nie;links">
        <rdfs:label>links</rdfs:label>
        <rdfs:comment>A linking relation. A piece of content links/mentions a piece of data</rdfs:comment>
        <rdfs:range rdf:resource="&nie;DataObject"/>
        <rdfs:domain rdf:resource="&nie;InformationElement"/>
        <rdfs:subPropertyOf rdf:resource="&nie;relatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/01/19/nie#relatedTo -->

    <owl:ObjectProperty rdf:about="&nie;relatedTo">
        <rdfs:label>relatedTo</rdfs:label>
        <rdfs:comment>A common superproperty for all relations between a piece of content and other pieces of data (which may be interpreted as other pieces of content).</rdfs:comment>
        <rdfs:range rdf:resource="&nie;DataObject"/>
        <rdfs:domain rdf:resource="&nie;InformationElement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/01/19/nie#url -->

    <owl:ObjectProperty rdf:about="&nie;url">
        <rdfs:domain rdf:resource="&nie;DataObject"/>
        <rdfs:range rdf:resource="&rdfs;Resource"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/03/22/nco#contributor -->

    <owl:ObjectProperty rdf:about="&nco;contributor">
        <rdfs:domain rdf:resource="&nie;InformationElement"/>
        <rdfs:range rdf:resource="&nco;Contact"/>
        <rdfs:subPropertyOf rdf:resource="&nao;contributor"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#belongsToContainer -->

    <owl:ObjectProperty rdf:about="&nfo;belongsToContainer">
        <rdfs:label>belongsToContainer</rdfs:label>
        <rdfs:comment>Models the containment relations between Files and Folders (or CompressedFiles).</rdfs:comment>
        <rdfs:domain rdf:resource="&nie;DataObject"/>
        <rdfs:subPropertyOf rdf:resource="&nie;isPartOf"/>
        <rdfs:range rdf:resource="&nfo;DataContainer"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#depiction -->

    <owl:ObjectProperty rdf:about="&nfo;depiction">
        <rdfs:label>depiction</rdfs:label>
        <rdfs:comment>Relates an information element to an image which depicts said element.</rdfs:comment>
        <rdfs:range rdf:resource="&nfo;Image"/>
        <nrl:inverseProperty rdf:resource="&nfo;depicts"/>
        <rdfs:domain rdf:resource="&rdfs;Resource"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#depicts -->

    <owl:ObjectProperty rdf:about="&nfo;depicts">
        <rdfs:label>depicts</rdfs:label>
        <rdfs:comment>Relates an image to the information elements it depicts.</rdfs:comment>
        <rdfs:domain rdf:resource="&nfo;Image"/>
        <nrl:inverseProperty rdf:resource="&nfo;depiction"/>
        <rdfs:range rdf:resource="&rdfs;Resource"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#fileOwner -->

    <owl:ObjectProperty rdf:about="&nfo;fileOwner">
        <rdfs:label>fileOwner</rdfs:label>
        <nrl:maxCardinality rdf:datatype="&xsd;integer">1</nrl:maxCardinality>
        <rdfs:comment>The owner of the file as defined by the file system access rights feature.</rdfs:comment>
        <rdfs:range rdf:resource="&nco;Contact"/>
        <rdfs:domain rdf:resource="&nfo;FileDataObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#fileUrl -->

    <owl:ObjectProperty rdf:about="&nfo;fileUrl">
        <rdfs:label>fileUrl</rdfs:label>
        <nao:deprecated rdf:datatype="&xsd;boolean">true</nao:deprecated>
        <rdfs:comment>URL of the file. It points at the location of the file. In cases where creating a simple file:// or http:// URL for a file is difficult (e.g. for files inside compressed archives) the applications are encouraged to use conventions defined by Apache Commons VFS Project at http://jakarta.apache.org/  commons/ vfs/ filesystems.html.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&nie;url"/>
        <rdfs:domain rdf:resource="&nfo;FileDataObject"/>
        <rdfs:range rdf:resource="&rdfs;Resource"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#hasMediaFileListEntry -->

    <owl:ObjectProperty rdf:about="&nfo;hasMediaFileListEntry">
        <rdfs:label>hasMediaFileListEntry</rdfs:label>
        <rdfs:comment>This property is intended to point to an RDF list of MediaFiles.</rdfs:comment>
        <rdfs:range rdf:resource="&nfo;MediaFileListEntry"/>
        <rdfs:domain rdf:resource="&nfo;MediaList"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nexif#artist -->

    <owl:ObjectProperty rdf:about="&nexif;artist">
        <rdfs:label>artist</rdfs:label>
        <rdfs:comment>tagNumber: 315
Person who created the image</rdfs:comment>
        <rdfs:range rdf:resource="&nco;Contact"/>
        <rdfs:domain rdf:resource="&nexif;Photo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nexif#gps -->

    <owl:ObjectProperty rdf:about="&nexif;gps">
        <rdfs:label>gps</rdfs:label>
        <rdfs:comment>The location where the picture has been made. This property aggregates values of two properties from the original EXIF specification: gpsLatitute (tag number 2) and gpsLongitude (tag number 4), and gpsAltitude (tag number 6).</rdfs:comment>
        <rdfs:domain rdf:resource="&nexif;Photo"/>
        <rdfs:range rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#Point"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#attachedPicture -->

    <owl:ObjectProperty rdf:about="&nid3;attachedPicture">
        <rdfs:label>attachedPicture</rdfs:label>
        <rdfs:comment>A picture attached to an audio file. The DataObject refered to by this property is usually interpreted as an nfo:Image Inspired by the attached picture tag defined in http://www.id3.org/id3v2.3.0 sec. 4.15)</rdfs:comment>
        <rdfs:range rdf:resource="&nie;DataObject"/>
        <rdfs:subPropertyOf rdf:resource="&nie;hasPart"/>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#backgroundArtist -->

    <owl:ObjectProperty rdf:about="&nid3;backgroundArtist">
        <rdfs:label>backgroundArtist</rdfs:label>
        <rdfs:comment>TPE2
The &apos;Band/Orchestra/Accompaniment&apos; frame is used for additional information about the performers in the recording.</rdfs:comment>
        <rdfs:range rdf:resource="&nco;Contact"/>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#composer -->

    <owl:ObjectProperty rdf:about="&nid3;composer">
        <rdfs:label>composer</rdfs:label>
        <rdfs:comment>TCOM
The &apos;Composer(s)&apos; frame is intended for the name of the composer(s). They are seperated with the &quot;/&quot; character.
Note that in the RDF representation each composer is represented with a separate triple.</rdfs:comment>
        <rdfs:range rdf:resource="&nco;Contact"/>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#generalEncapsulatedObject -->

    <owl:ObjectProperty rdf:about="&nid3;generalEncapsulatedObject">
        <rdfs:label>generalEncapsulatedObject</rdfs:label>
        <rdfs:comment>An arbitrary file embedded in an audio file. Inspired by http://www.id3.org/id3v2.3.0 sec. 
4.16)</rdfs:comment>
        <rdfs:range rdf:resource="&nie;DataObject"/>
        <rdfs:subPropertyOf rdf:resource="&nie;hasPart"/>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#involvedPerson -->

    <owl:ObjectProperty rdf:about="&nid3;involvedPerson">
        <rdfs:label>involvedPerson</rdfs:label>
        <rdfs:comment>Links an ID3 file to an InvolvedPerson, an equivalent of the involvedPeopleList tag. Since there might be a lot of people contributing to an audio file in various ways, such as musicians and technicians, the &apos;Text information frames&apos; are often insufficient to list everyone involved in a project. The &apos;Involved people list&apos; is a frame containing the names of those involved, and how they were involved. The body simply contains a terminated string with the involvement directly followed by a terminated string with the involvee followed by a new involvement and so on. There may only be one &quot;IPLS&quot; frame in each tag.</rdfs:comment>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
        <rdfs:range rdf:resource="&nid3;InvolvedPerson"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#involvedPersonContact -->

    <owl:ObjectProperty rdf:about="&nid3;involvedPersonContact">
        <rdfs:label>involvedPersonContact</rdfs:label>
        <rdfs:comment>An actual contact to the involved person.</rdfs:comment>
        <rdfs:range rdf:resource="&nco;Contact"/>
        <rdfs:domain rdf:resource="&nid3;InvolvedPerson"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#leadArtist -->

    <owl:ObjectProperty rdf:about="&nid3;leadArtist">
        <rdfs:label>leadArtist</rdfs:label>
        <rdfs:comment>TPE1
The &apos;Lead artist(s)/Lead performer(s)/Soloist(s)/Performing group&apos; is used for the main artist(s). They are seperated with the &quot;/&quot; character.</rdfs:comment>
        <rdfs:range rdf:resource="&nco;Contact"/>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#licensee -->

    <owl:ObjectProperty rdf:about="&nid3;licensee">
        <rdfs:label>licensee</rdfs:label>
        <rdfs:comment>TOWN
The &apos;File owner/licensee&apos; frame contains the name of the owner or licensee of the file and it&apos;s contents.</rdfs:comment>
        <rdfs:range rdf:resource="&nco;Contact"/>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#officialArtistWebpage -->

    <owl:ObjectProperty rdf:about="&nid3;officialArtistWebpage">
        <rdfs:label>officialArtistWebpage</rdfs:label>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
        <rdfs:range rdf:resource="&rdfs;Resource"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#officialAudioSourceWebpage -->

    <owl:ObjectProperty rdf:about="&nid3;officialAudioSourceWebpage">
        <rdfs:label>officialAudioSourceWebpage</rdfs:label>
        <rdfs:comment>WOAS
The &apos;Official audio source webpage&apos; frame is a URL pointing at the official webpage for the source of the audio file, e.g. a movie.</rdfs:comment>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
        <rdfs:range rdf:resource="&rdfs;Resource"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#officialFileWebpage -->

    <owl:ObjectProperty rdf:about="&nid3;officialFileWebpage">
        <rdfs:label>officialFileWebpage</rdfs:label>
        <rdfs:comment>WOAF
The &apos;Official audio file webpage&apos; frame is a URL pointing at a file specific webpage.</rdfs:comment>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
        <rdfs:range rdf:resource="&rdfs;Resource"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#originalArtist -->

    <owl:ObjectProperty rdf:about="&nid3;originalArtist">
        <rdfs:label>originalArtist</rdfs:label>
        <rdfs:comment>TOPE
The &apos;Original artist(s)/performer(s)&apos; frame is intended for the performer(s) of the original recording, if for example the music in the file should be a cover of a previously released song. The performers are seperated with the &quot;/&quot; character.
Note that in the RDF repressentation each orignal artist is represented with a separate triple.</rdfs:comment>
        <rdfs:range rdf:resource="&nco;Contact"/>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#originalTextWriter -->

    <owl:ObjectProperty rdf:about="&nid3;originalTextWriter">
        <rdfs:label>originalTextWriter</rdfs:label>
        <rdfs:comment>TOLY
The &apos;Original lyricist(s)/text writer(s)&apos; frame is intended for the text writer(s) of the original recording, if for example the music in the file should be a cover of a previously released song. The text writers are seperated with the &quot;/&quot; character.
Note that in the RDF representation each original lyricist is represented with a separate triple.</rdfs:comment>
        <rdfs:range rdf:resource="&nco;Contact"/>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#publisher -->

    <owl:ObjectProperty rdf:about="&nid3;publisher">
        <rdfs:label>publisher</rdfs:label>
        <rdfs:comment>TPUB
The &apos;Publisher&apos; frame simply contains the name of the label or publisher.</rdfs:comment>
        <rdfs:range rdf:resource="&nco;Contact"/>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#publishersWebpage -->

    <owl:ObjectProperty rdf:about="&nid3;publishersWebpage">
        <rdfs:label>publishersWebpage</rdfs:label>
        <rdfs:comment>WPUB
The &apos;Publishers official webpage&apos; frame is a URL pointing at the official wepage for the publisher.</rdfs:comment>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
        <rdfs:range rdf:resource="&rdfs;Resource"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/05/10/nid3#textWriter -->

    <owl:ObjectProperty rdf:about="&nid3;textWriter">
        <rdfs:label>textWriter</rdfs:label>
        <rdfs:comment>TEXT
The &apos;Lyricist(s)/Text writer(s)&apos; frame is intended for the writer(s) of the text or lyrics in the recording. They are seperated with the &quot;/&quot; character.
Note that in the RDF representation each text writer is represented with a separate triple.</rdfs:comment>
        <rdfs:range rdf:resource="&nco;Contact"/>
        <rdfs:domain rdf:resource="&nid3;ID3Audio"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2007/08/15/nao#contributor -->

    <owl:ObjectProperty rdf:about="&nao;contributor"/>
    


    <!-- http://www.semanticdesktop.org/ontologies/2009/02/19/nmm#actor -->

    <owl:ObjectProperty rdf:about="&nmm;actor">
        <rdfs:label>nmm:actor</rdfs:label>
        <rdfs:range rdf:resource="&nco;Contact"/>
        <rdfs:subPropertyOf rdf:resource="&nco;contributor"/>
        <rdfs:domain rdf:resource="&nfo;Video"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticdesktop.org/ontologies/2009/02/19/nmm#musicAlbum -->

    <owl:ObjectProperty rdf:about="&nmm;musicAlbum">
        <rdfs:label>nmm:musicAlbum</rdfs:label>
        <nrl:maxCardinality rdf:datatype="&xsd;integer">1</nrl:maxCardinality>
        <rdfs:comment>Album the music belongs to</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&nie;isLogicalPartOf"/>
        <rdfs:range rdf:resource="&nmm;MusicAlbum"/>
        <rdfs:domain rdf:resource="&nmm;MusicPiece"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/syed.rahman/ontologies/2016/1/SFS-Ontology#belongedToContainer -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/syed.rahman/ontologies/2016/1/SFS-Ontology#belongedToContainer">
        <rdfs:label>sfs:belongedToContainer</rdfs:label>
        <rdfs:comment>Previous container of a file. Models the containment relations between Files and Folders (or compressed files) where they were previously been in. A user group semantically related files in a folder. If a user place a file in one folder and then after some time he moves it to another folder then it indicates that files in both folders are somehow related to each other. Tracking previous containers/folders of a file enables to relate a file with the files of containers where it previously belonged to.</rdfs:comment>
        <rdfs:domain rdf:resource="&nie;DataObject"/>
        <rdfs:range rdf:resource="&nfo;DataContainer"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/syed.rahman/ontologies/2016/1/SFS-Ontology#fileAccessedLocation_Time -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/syed.rahman/ontologies/2016/1/SFS-Ontology#fileAccessedLocation_Time">
        <rdfs:label>sfs:fileAccessedLocation_Time</rdfs:label>
        <rdfs:comment>It relates objects of nfo:FileDataObject and FileAccess classes. Which enables to track accessed time and location of a file. So, frequent access timings and locations can be calculated.</rdfs:comment>
        <rdfs:domain rdf:resource="&nfo;FileDataObject"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/syed.rahman/ontologies/2016/1/SFS-Ontology#FileAccess"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/syed.rahman/ontologies/2016/1/SFS-Ontology#fileCreatedByApplication -->

