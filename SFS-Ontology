    <!-- http://www.semanticweb.org/syed.rahman/ontologies/2016/1/SFS-Ontology#fileNearDuplicateOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/syed.rahman/ontologies/2016/1/SFS-Ontology#fileNearDuplicateOf">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label>sfs:fileNearDuplicateOf</rdfs:label>
        <rdfs:comment>Approximate match. Files that are identical to some extent. These could be older versions of the file. Use a similarity threshold to detect near-duplicates. e.g. If Similarity between two files is &gt; 80%, then both files are &quot;near duplicates&quot;.</rdfs:comment>
        <rdfs:range rdf:resource="&nfo;FileDataObject"/>
        <rdfs:domain rdf:resource="&nfo;FileDataObject"/>
    </owl:ObjectProperty>
    


