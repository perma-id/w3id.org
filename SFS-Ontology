    <!-- http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#ArchiveItem -->

    <owl:Class rdf:about="&nfo;ArchiveItem">
        <rdfs:label>nfo:ArchiveItem</rdfs:label>
