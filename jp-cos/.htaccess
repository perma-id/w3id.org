Options +FollowSymLinks

RewriteEngine on

RewriteCond "%{HTTP_ACCEPT}" "text/turtle"
RewriteRule ^([0-9][0-9a-zA-Z]{2})([0-9a-zA-Z]{13})$ https://jp-cos.github.io/$1/$2.ttl [R=302,L]

RewriteCond "%{HTTP_ACCEPT}" "text/turtle"
RewriteRule ^(.*)$ https://jp-cos.github.io/$1.ttl [R=302,L]

RewriteRule ^([0-9][0-9a-zA-Z]{2})([0-9a-zA-Z]{13})\.ttl$ https://jp-cos.github.io/$1/$2.ttl [R=302,L]

RewriteRule ^([0-9][0-9a-zA-Z]{2})([0-9a-zA-Z]{13})$ https://jp-cos.github.io/$1/$2 [R=302,L]

RewriteRule ^(.*) https://jp-cos.github.io/$1 [R=302,L]
