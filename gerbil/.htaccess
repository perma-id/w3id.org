Options +FollowSymLinks
RewriteEngine on
RewriteRule "^swc2017/sparql(.*)$" "http://gerbil-swchallenge.cs.uni-paderborn.de:8080/sparql$1" [R=302,L]
RewriteRule "^swc2017(.*)$" "http://gerbil-swchallenge.cs.uni-paderborn.de:8080/gerbil$1" [R=302,L]
RewriteRule "^kbc/sparql(.*)$" "http://gerbil-swchallenge.cs.uni-paderborn.de:8080/sparql$1" [R=302,L]
RewriteRule "^kbc(.*)$" "http://gerbil-swchallenge.cs.uni-paderborn.de:8080/gerbil$1" [R=302,L]
RewriteRule "^qa/sparql(.*)$" "http://gerbil-qa.cs.uni-paderborn.de:8080/sparql$1" [R=302,L]
RewriteRule "^qa(.*)$" "http://gerbil-qa.cs.uni-paderborn.de:8080/gerbil$1" [R=302,L]
# NLG/BENG is offline at the moment
#RewriteRule "^nlg/sparql(.*)$" "https://gerbil-nlg.dice-research.org/sparql$1" [R=302,L]
#RewriteRule "^nlg(.*)$" "https://gerbil-nlg.dice-research.org/gerbil$1" [R=302,L]
RewriteRule "^sparql(.*)$" "http://gerbil.cs.uni-paderborn.de:8080/sparql$1" [R=302,L]
