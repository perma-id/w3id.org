Header set Access-Control-Allow-Origin *
Options +FollowSymLinks
RewriteEngine on

RewriteRule "^schema.*$" "https://migrationskb.github.io/MGKB/migrationsKB/documentation.html" [R=302,L]
RewriteRule "^sparql.*$" "https://mgkb.fiz-karlsruhe.de/sparql/" [R=302,L]
RewriteRule "^codes.*$" "https://github.com/migrationsKB/MGKB" [R=302,L]
RewriteRule "^data.*$" "https://zenodo.org/record/5206820#.YRqF1nUza0o" [R=302,L]


RewriteRule ^(.*)$ https://migrationskb.github.io/MGKB/ [R=302,L]
