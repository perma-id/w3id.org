Header set Access-Control-Allow-Origin *
Header set Access-Control-Allow-Headers DNT,X-Mx-ReqToken,Keep-Alive,User-Agent,X-Requested-With,If-Modified$
Options +FollowSymLinks
RewriteEngine on
RewriteRule ^$ https://www.ugent.be/ea/idlab/en/ [R=302,L]
RewriteRule ^unshacled$ http://ecodalo.ilabt.imec.be:8980/ [R=302,L]
RewriteRule ^unshacled/app$ http://ecodalo.ilabt.imec.be:8980/ [R=302,L]
RewriteRule ^unshacled/screencast$ http://users.ugent.be/~bjdmeest/voila2018/unshacled.mp4 [R=302,L]
RewriteRule ^unshacled/screencast/voila2018$ http://users.ugent.be/~bjdmeest/voila2018/unshacled.mp4 [R=302,L]
RewriteRule ^unshacled/spec/shape-uml/$ https://knowledgeonwebscale.github.io/shapeuml-spec/ [R=302,L]
RewriteRule ^unshacled/spec/shape-vowl/$ https://knowledgeonwebscale.github.io/shapevowl-spec/ [R=302,L]
RewriteRule ^unshacled/(.*)$ https://lov.ilabt.imec.be/unshacled/$1 [R=302,L]
RewriteRule ^ns/oslo-steps(.*)$ https://fast.ilabt.imec.be/ns/oslo-steps$1 [R=302,L]
RewriteRule ^oslo-steps/api/docs(?:/?)(.*)$ https://fast.ilabt.imec.be/api/v10/api-docs/$1 [R=302,L]
RewriteRule ^idlab/function.*$ https://github.com/FnOio/idlab-functions-java/raw/main/src/main/resources/fno/functions_idlab.ttl [R=307,L]
