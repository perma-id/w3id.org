Options +FollowSymLinks
RewriteEngine on

RewriteRule ^(.*).yaml$ https://raw.githubusercontent.com/microbiomedata/nmdc-schema/main/src/schema/$1.yaml [R=307,L]

RewriteRule ^((?:act|ansm|bsm|bsmprc|clsite|dobj|extrp|frsite|inst|libprp|matsm|omprc|poolp|procsm|sty|wf).*)$      https://api.microbiomedata.org/nmdcschema/ids/$1 [R=307,L]

RewriteRule ^(.*)$      https://microbiomedata.github.io/nmdc-schema/$1 [R=307,L]
