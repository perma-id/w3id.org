Options +FollowSymLinks
RewriteEngine on
RewriteRule ^\/?aux-end\/([^/]*)\/(.*)$ /mtv/$2$1 [N]
RewriteRule ^\/?aux-dom\/([^/]*)\/(.*)$ https://$1/$2 [R=302,L]
RewriteRule ^\/?aux-sfx-dom\/([^/]*)\/(.*)\/([^/]*)$ https://$2/$3$1 [R=302,L]
RewriteRule ^\/?(FISE/map-data-(RPP|MHS)/v0-3-2/internet/.*)$ https://purl.org/mtv/$1 [R=302,L]
RewriteRule ^\/?(.*)$ http://mfkp.org/doi/$1 [R=302,L]
