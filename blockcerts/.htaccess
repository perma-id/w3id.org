Header set Access-Control-Allow-Origin *
Header set Access-Control-Allow-Headers DNT,X-Mx-ReqToken,Keep-Alive,User-Agent,X-Requested-With,If-Modified$
Options +FollowSymLinks
RewriteEngine on
RewriteRule ^$ https://www.blockcerts.org/ [R=302,L]
RewriteRule ^v1$ https://www.blockcerts.org/schema/1.2/context.json [R=302,L]
RewriteRule ^v2$ https://www.blockcerts.org/schema/2.0/context.json [R=302,L]
RewriteRule ^v2\.1$ https://www.blockcerts.org/schema/2.1/context.json [R=302,L]
RewriteRule ^v3\.0-alpha$ https://www.blockcerts.org/schema/3.0-alpha/context.json [R=302,L]
RewriteRule ^v3\.0-beta$ https://www.blockcerts.org/schema/3.0-beta/context.json [R=302,L]
RewriteRule ^v3$ https://www.blockcerts.org/schema/3.0/context.json [R=302,L]
RewriteRule ^v3\.1$ https://www.blockcerts.org/schema/3.1/context.json [R=302,L]
RewriteRule ^(.*)$ https://www.blockcerts.org/$1 [R=302,L]
