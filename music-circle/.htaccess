Options +FollowSymLinks
# Turn off MultiViews
Options -MultiViews

RewriteEngine On

AddType application/rdf+xml .rdf
AddType application/ld+json .json
AddType text/turtle .ttl
AddType text/rdf+n3 .n3

####################################################################################
#                  Music Circle Platform Ontology rules                            #
####################################################################################

# namespace URI

# Rewrite rule to serve directed HTML content from class/prop URIs
RewriteCond %{HTTP_ACCEPT} !application/rdf\+xml.*(text/html|application/xhtml\+xml) [OR]
RewriteCond %{HTTP_ACCEPT} text/html [OR] 
RewriteCond %{HTTP_ACCEPT} application/xhtml\+xml [OR] 
RewriteCond %{HTTP_USER_AGENT} ^Mozilla/.* 
RewriteRule ^platform-ontology/(.+)/$ http://eecs.qmul.ac.uk/~thomasw/music-circle/platform-ontology/$1/index-en.html  [R=303] 

# Rewrite rule to serve RDF/XML content from the vocabulary URI if requested
RewriteCond %{HTTP_ACCEPT} application/rdf\+xml 
RewriteRule ^platform-ontology/(.+)/$ http://eecs.qmul.ac.uk/~thomasw/music-circle/platform-ontology/$1/mcp.rdf  [R=303]

# Rewrite rule to serve Turtle content from the vocabulary URI if requested
RewriteCond %{HTTP_ACCEPT} text/turtle  
RewriteRule ^platform-ontology/(.+)/$ http://eecs.qmul.ac.uk/~thomasw/music-circle/platform-ontology/$1/mcp.ttl  [R=303]

# Rewrite rule to serve JSON content from the vocabulary URI if requested
RewriteCond %{HTTP_ACCEPT} application/json 
RewriteRule ^platform-ontology/(.+)/$ http://eecs.qmul.ac.uk/~thomasw/music-circle/platform-ontology/$1/mcp.json [R=303]

# Rewrite rule to serve N3 content from the vocabulary URI if requested
RewriteCond %{HTTP_ACCEPT} text/rdf\+n3 
RewriteRule ^platform-ontology/(.+)/$ http://eecs.qmul.ac.uk/~thomasw/music-circle/platform-ontology/$1/mcp.n3 [R=303]

#default response 
RewriteRule ^platform-ontology/(.+)/$ http://eecs.qmul.ac.uk/~thomasw/music-circle/platform-ontology/$1/index-en.html  [R=303] 


# class/prop URIs

# Rewrite rule to serve directed HTML content from class/prop URIs
RewriteCond %{HTTP_ACCEPT} !application/rdf\+xml.*(text/html|application/xhtml\+xml) [OR]
RewriteCond %{HTTP_ACCEPT} text/html [OR] 
RewriteCond %{HTTP_ACCEPT} application/xhtml\+xml [OR] 
RewriteCond %{HTTP_USER_AGENT} ^Mozilla/.* 
RewriteRule ^platform-ontology/(.+)/(.+) http://eecs.qmul.ac.uk/~thomasw/music-circle/platform-ontology/$1/index-en.html#$2  [R=303,NE] 

# Rewrite rule to serve RDF/XML content from the vocabulary URI if requested
RewriteCond %{HTTP_ACCEPT} application/rdf\+xml 
RewriteRule ^platform-ontology/(.+)/(.+) http://eecs.qmul.ac.uk/~thomasw/music-circle/platform-ontology/$1/mcp.rdf  [R=303]

# Rewrite rule to serve Turtle content from the vocabulary URI if requested
RewriteCond %{HTTP_ACCEPT} text/turtle  
RewriteRule ^platform-ontology/(.+)/(.+) http://eecs.qmul.ac.uk/~thomasw/music-circle/platform-ontology/$1/mcp.ttl  [R=303]

# Rewrite rule to serve JSON content from the vocabulary URI if requested
RewriteCond %{HTTP_ACCEPT} application/json 
RewriteRule ^platform-ontology/(.+)/(.+) http://eecs.qmul.ac.uk/~thomasw/music-circle/platform-ontology/$1/mcp.json [R=303]

# Rewrite rule to serve N3 content from the vocabulary URI if requested
RewriteCond %{HTTP_ACCEPT} text/rdf\+n3 
RewriteRule ^platform-ontology/(.+)/(.+) http://eecs.qmul.ac.uk/~thomasw/music-circle/platform-ontology/$1/mcp.n3 [R=303]

#default response 
RewriteRule ^platform-ontology/(.+)/(.+) http://eecs.qmul.ac.uk/~thomasw/music-circle/platform-ontology/$1/index-en.html#$2  [R=303,NE] 



