RewriteEngine on

# 2. Machine readable: TriG | Turtle | RDF/XML
RewriteCond %{HTTP_ACCEPT} application/trig
RewriteRule ^def$ https://bimloket.github.io/nta8035/data/concat/nta8035.trig [R=302,L]

RewriteCond %{HTTP_ACCEPT} text/turtle [OR]
RewriteCond %{HTTP_ACCEPT} text/n3
RewriteRule ^def$ https://bimloket.github.io/nta8035/data/concat/nta8035.ttl [R=302,L]

RewriteCond %{HTTP_ACCEPT} application/rdf\+xml [OR]
RewriteCond %{HTTP_ACCEPT} application/owl\+xml
RewriteRule ^def$ https://bimloket.github.io/nta8035/data/concat/nta8035.rdf [R=302,L]

# 3. Human readable: HTML
RewriteCond %{HTTP_ACCEPT} text/html [OR]
RewriteCond %{HTTP_USER_AGENT} ^Mozilla/.*
RewriteRule ^def$ https://bimloket.github.io/nta8035/def [R=302,L]

# 4. Default: Turtle
RewriteRule ^$ https://bimloket.github.io/nta8035/data/nta8035.ttl [R=302,L]
