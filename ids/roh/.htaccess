RewriteRule ^university-hr\/uk\/?$ https://herculescrue.github.io/ROH/roh/university-hr/university-hr-uk.ttl [R=302,L]
RewriteRule ^university-structure\/?$ https://herculescrue.github.io/ROH/roh/university-structure/university-structure.ttl [R=302,L]
# Mirrored ontologies
RewriteRule ^mirror\/dc\/terms\/?$ https://herculescrue.github.io/ROH/mirror/DCTerms/dcterms.rdf [R=302,L]
RewriteRule ^mirror\/cerif\/?$ https://herculescrue.github.io/ROH/mirror/cerif/cerif.rdf [R=302,L]
RewriteRule ^mirror\/foaf\/?$ https://herculescrue.github.io/ROH/mirror/foaf/foaf.rdf [R=302,L]
RewriteRule ^mirror\/geonames\/?$ https://herculescrue.github.io/ROH/mirror/geonames/geonames.rdf [R=302,L]
RewriteRule ^mirror\/obo\/bfo\/?$ https://herculescrue.github.io/ROH/mirror/obo/bfo.owl [R=302,L]
RewriteRule ^mirror\/obo\/ero\/?$ https://herculescrue.github.io/ROH/mirror/obo/ero.owl [R=302,L]
RewriteRule ^mirror\/obo\/iao\/?$ https://herculescrue.github.io/ROH/mirror/obo/iao.owl [R=302,L]
RewriteRule ^mirror\/obo\/ro\/?$ https://herculescrue.github.io/ROH/mirror/obo/ro.owl [R=302,L]
RewriteRule ^mirror\/org\/? https://herculescrue.github.io/ROH/mirror/org/org.rdf [R=302,L]
RewriteRule ^mirror\/skos\/?$ https://herculescrue.github.io/ROH/mirror/skos/skos.rdf [R=302,L]
RewriteRule ^mirror\/swrc\/?$ https://herculescrue.github.io/ROH/mirror/swrc/swrc.owl [R=302,L]
