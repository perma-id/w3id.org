RewriteRule ^unesco-individuals\/?$ https://herculescrue.github.io/ROH/roh/knowledge-area/unesco-knowledge-area.rdf [R=302,L]
RewriteRule ^project-classification\/?$ https://herculescrue.github.io/ROH/roh/project-classification/project-classification.owl [R=302,L]
RewriteRule ^university-hr\/es\/?$ https://herculescrue.github.io/ROH/roh/university-hr/university-hr-es.ttl [R=302,L]
