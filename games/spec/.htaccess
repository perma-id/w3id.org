Options +FollowSymLinks
RewriteEngine on
#Rewrite rules for Asteroids vocabulary
RewriteCond %{HTTP_ACCEPT} ^.*application/rdf\+xml.* 
RewriteRule ^asteroids$ http://dgarijo.github.io/VideoGameOntology/GameExtensions/AsteroidsOntology/asteroids.owl [R=303,L]
RewriteCond %{HTTP_ACCEPT} ^.*text/turtle.* 
RewriteRule ^asteroids$ http://dgarijo.github.io/VideoGameOntology/GameExtensions/AsteroidsOntology/asteroids.ttl [R=303,L]
RewriteRule ^asteroids$ http://dgarijo.github.io/VideoGameOntology/GameExtensions/AsteroidsOntology/asteroidsDoc/ [R=303,L]

#Rewrite rules for Coil vocabulary extension 
RewriteCond %{HTTP_ACCEPT} ^.*application/rdf\+xml.* 
RewriteRule ^coil$ http://dgarijo.github.io/VideoGameOntology/GameExtensions/CoilOntology/coil.owl [R=303,L]
RewriteCond %{HTTP_ACCEPT} ^.*text/turtle.* 
RewriteRule ^coil$ http://dgarijo.github.io/VideoGameOntology/GameExtensions/CoilOntology/coil.ttl [R=303,L]
RewriteRule ^coil$ http://dgarijo.github.io/VideoGameOntology/GameExtensions/CoilOntology/coilDoc/ [R=303,L]

#Rewrite rules for browserquest
RewriteCond %{HTTP_ACCEPT} ^.*application/rdf\+xml.* 
RewriteRule ^browserquest$ http://dgarijo.github.io/VideoGameOntology/GameExtensions/BrowserQuestOntology/browserQuest.owl [R=303,L]
RewriteCond %{HTTP_ACCEPT} ^.*text/turtle.* 
RewriteRule ^browserquest$ http://dgarijo.github.io/VideoGameOntology/GameExtensions/BrowserQuestOntology/browserQuest.ttl [R=303,L]
RewriteRule ^browserquest$ http://dgarijo.github.io/VideoGameOntology/GameExtensions/BrowserQuestOntology/browserDoc/ [R=303,L]