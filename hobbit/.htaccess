Options +FollowSymLinks
RewriteEngine on
RewriteRule "^vocab$" "https://raw.githubusercontent.com/hobbit-project/ontology/master/ontology.ttl" [R=302,L]
RewriteRule "^vocab#(.*)$" "https://raw.githubusercontent.com/hobbit-project/ontology/master/ontology.ttl" [R=302,L]
RewriteRule "^error$" "https://raw.githubusercontent.com/hobbit-project/ontology/master/errors.ttl" [R=302,L]
RewriteRule "^error#(.*)$" "https://raw.githubusercontent.com/hobbit-project/ontology/master/errors.ttl" [R=302,L]
RewriteRule "^experiments#(.*)$" "http://master.project-hobbit.eu/#/experiments/details?id=$1" [R=302,L]
RewriteRule "^challenges#(.*)$" "http://master.project-hobbit.eu/#/challenges/$1" [R=302,L]
RewriteRule "^(.*)$" "https://project-hobbit.eu/" [R=302,L]
