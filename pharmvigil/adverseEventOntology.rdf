    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#administrationRoute -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#administrationRoute">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Product"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The route of administration of the object (Drug or DrugClass or DrugOther).</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#administrationTime -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#administrationTime">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Product"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#real"/>
        <rdfs:comment xml:lang="en">The total duration of administration of the object (Drug or DrugClass or DrugOther), representing the time in days or months or years.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#ageDemographics -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#ageDemographics">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#LivingThing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#real"/>
        <rdfs:comment xml:lang="en">A number representing the age in days or months or years or age group of the object (Human or HumanGroup or Vertebrate or VertebrateGroup or InvertebrateGroup)</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#drugDose -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#drugDose">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Product"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#real"/>
        <rdfs:comment xml:lang="en">The specific amount of object (Drug or DrugClass or DrugOther).</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#durationOfEvent -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#durationOfEvent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Dysfunction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#real"/>
        <rdfs:comment xml:lang="en">The total time during which the object (BiologicalDisorderInduced or AdverseState or BiologicalDisorder) has been active or progressive. It may represent the time in days or months or years.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#eventOutcome -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#eventOutcome">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Dysfunction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The result or outcome of the object (BiologicalDisorderInduced or AdverseState) that was induced.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#genderDemographics -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#genderDemographics">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#LivingThing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Phenotypic representation of a person or the object (Human or HumanGroup or Vertebrate or VertebrateGroup or InvertebrateGroup)</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#pregnancyCondition -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#pregnancyCondition">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Human"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#HumanGroup"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Vertebrate"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#VertebrateGroup"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Condition of the object (Human or HumanGroup or Vertebrate or VertebrateGroup), whether the object is pregnant or not. It is specific to female.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#raceDemographics -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#raceDemographics">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Human"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#HumanGroup"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
