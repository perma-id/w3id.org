<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#"
     xml:base="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:pharmvigil="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil">
        <owl:versionIRI rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil/1.0"/>
        <rdfs:comment xml:lang="en">A structured framework for automating literature using classes, properties, and restrictions that form the ontology. This ontology specification serves as the foundation for automated learning and review of literature articles.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#addedWith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#addedWith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Product"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Product"/>
        <owl:propertyDisjointWith rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#combinedWith"/>
        <rdfs:comment xml:lang="en">A relation formed when one object (Drug or DrugClass or DrugOther) has been administered along with another object (Drug or DrugClass or DrugOther) but not in a fixed dose combination. It denotes the suspect product and the concomitant products used for the treatment of one or many diseases or disorders in human or animal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#adverseEffectFor -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#adverseEffectFor">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#medicalHistoryOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#AdverseState"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#BiologicalDisorderInduced"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#LivingThing"/>
        <owl:propertyDisjointWith rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#medicalHistoryOf"/>
        <rdfs:comment xml:lang="en">A relation formed when one object (BiologicalDisorderInduced or AdverseState) has occured in another object (Human or Human_group or Vertebrate or VertebrateGroup or InvertebrateGroup). It denotes the disease or disorder induced by the product has occurred in a human or animal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#adverseEffectOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#adverseEffectOf">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#improvedOnUsing"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#AdverseState"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#BiologicalDisorderInduced"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Drug"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#DrugClass"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#DrugOther"/>
        <rdfs:comment xml:lang="en">A relation formed when one object (BiologicalDisorderInduced or AdverseState) was caused or induced by another object (Drug or DrugClass or DrugOther). It denotes that an undesirable or harmful effect was caused by the product which was used for the treatment of an already existing disease or an already existing disorder or prevention of a disease.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#alsoKnownAs -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#alsoKnownAs">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Dysfunction"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#LivingThing"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Product"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Dysfunction"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#LivingThing"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Product"/>
        <rdfs:comment xml:lang="en">A relation formed when one object (Drug or DrugClass or DrugOther or BiologicalDisorder or BiologicalDisorderInduced or AdverseState or Human or Human_group or Vertebrate or VertebrateGroup or InvertebrateGroup) is also known as or referred or abbreviated or called as the same object (Drug or DrugClass or DrugOther or BiologicalDisorder or BiologicalDisorderInduced or AdverseState or Human or Human_group or Vertebrate or VertebrateGroup or InvertebrateGroup). It denotes that one entity has been called by some other name of the entity. It also denotes that the same entity has two or more names and abbreviations also.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#combinedWith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#combinedWith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Drug"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#DrugClass"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Drug"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#DrugClass"/>
        <rdfs:comment xml:lang="en">A relation formed when one object (Drug or DrugClass) has been administered along with another object (Drug or DrugClass) in a fixed dose combination. It denotes that two or more products are combined and used together for the treatment of one or many diseases or disorders in human or animal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#componentOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#componentOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Drug"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#DrugClass"/>
        <rdfs:comment xml:lang="en">A relation formed when one object (Drug) is a component or member of another object (DrugClass). It denotes the class to which the product belongs to.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#improvedOnUsing -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#improvedOnUsing">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#inducedAdverseEffect"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Dysfunction"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Product"/>
        <owl:propertyDisjointWith rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#inducedAdverseEffect"/>
        <rdfs:comment xml:lang="en">A relation formed when one object (BiologicalDisorder or BiologicalDisorderInduced or AdverseState) has improved or was cured by another object (Drug or DrugClass or DrugOther). It denotes the disease or disorder which was treated by the product has inproved or got better or resolved.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#inducedAdverseEffect -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#inducedAdverseEffect">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#isIndicatedFor"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Product"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#AdverseState"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#BiologicalDisorderInduced"/>
        <rdfs:comment xml:lang="en">A relation formed when one object (Drug or DrugClass or DrugOther) has caused or induced another object (BiologicalDisorderInduced or AdverseState). It denotes the product used for the treatment of a disease or disorder or for prevention of a disease has caused another undesirable or harmful effect.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#isIndicatedFor -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#isIndicatedFor">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Product"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Dysfunction"/>
        <rdfs:comment xml:lang="en">A relation formed when one object (Drug or DrugClass or DrugOther) is used for treating another object (BiologicalDisorder or BiologicalDisorderInduced or AdverseState). It denotes the product used for the treatment of a disease or disorder or for prevention of a disease.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#manifestsAs -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#manifestsAs">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Dysfunction"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#BiologicalDisorder"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#BiologicalDisorderInduced"/>
        <rdfs:comment xml:lang="en">A relation formed when one object (BiologicalDisorder or BiologicalDisorderInduced or AdverseState) produced or developed or complicated into another object (BiologicalDisorder or BiologicalDisorderInduced). It denotes that the symptoms and manifestations of a disease or condition has occurred in a patient.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#medicalHistoryOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#medicalHistoryOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#BiologicalDisorder"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#LivingThing"/>
        <rdfs:comment xml:lang="en">A relation formed when one object (BiologicalDisorder) occured in another object (Human or Human_group or Vertebrate or VertebrateGroup or InvertebrateGroup). It denotes that the disease or condition has occurred in a patient and the patient is getting treated or has already been treated for it.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#wasExposedTo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#wasExposedTo">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#wasTreatedWith"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#LivingThing"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Product"/>
        <owl:propertyDisjointWith rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#wasTreatedWith"/>
        <rdfs:comment xml:lang="en">A relation formed when one object (Human or Human_group or Vertebrate or VertebrateGroup or InvertebrateGroup) is exposed to another object (Drug or DrugClass or DrugOther) accidentally or intentionally but definitely not for treating a disease or disorder or for prevention of a disease. It denotes that the product was not used to treat a human or animal and was exposed to the human or animal for non-therapeutic purposes or by accident.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#wasTreatedWith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#wasTreatedWith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#LivingThing"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/umar/ontologies/2025/0/pharmvigil#Product"/>
        <rdfs:comment xml:lang="en">A relation formed when one object (Human or Human_group or Vertebrate or VertebrateGroup or InvertebrateGroup) is treated with another object (Drug or DrugClass or DrugOther) for a disease or disorder or prevention. It denotes that the product was used to treat a human or animal.</rdfs:comment>
    </owl:ObjectProperty>
    


